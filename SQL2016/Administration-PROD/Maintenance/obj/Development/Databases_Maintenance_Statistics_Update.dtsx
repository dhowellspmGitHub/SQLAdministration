          <ProjectItem
            Name="ScriptMain.vb"
            Encoding="UTF8"><![CDATA[' Microsoft SQL Server Integration Services Script Task
' Write scripts using Microsoft Visual Basic 2008.
' The ScriptMain is the entry point class of the script.

Imports System
Imports System.Data
Imports System.Math
Imports Microsoft.SqlServer.Dts.Runtime

<Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute> _
<System.CLSCompliantAttribute(False)> _
Partial Public Class ScriptMain
	Inherits Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase

	Enum ScriptResults
		Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
		Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
	End Enum
	
   
    Public Sub Main()
		'RO Variables: User::Databases_ProcessCompletedCount,User::Databases_ProcessedCount,User::Databases_TotalCount,User::ErrorHandling_FailedDatabases_Count,User::ErrorHandling_FailedStatsUpdate_TotalCount,User::ErrorHandling_PersistentText,User::ErrorHandling_SendEmailOnSuccess,System::ExecutionInstanceGUID,System::PackageName,$Package::ErrorHandling_SendEmail,$Package::InitiatingJobName,$Project::Database_Name_01,$Project::Server_Name_01,$Project::Server_Name_02,$Project::Login_Name_02

		'RW Variables: User::Email_Source,User::Email_Subject,User::Email_ToLine,User::ErrorHandling_SendEmail
		Dim strUserName As String = Dts.Variables("$Project::Login_Name_02").Value.ToString
		Dim bolSendEmailOverride As Boolean = CBool(Dts.Variables("$Package::ErrorHandling_SendEmail").Value)
		Dim bolSendEmailOnSuccess As Boolean = CBool(Dts.Variables("User::ErrorHandling_SendEmailOnSuccess").Value)
		Dim strPersistentErrorText As String = Dts.Variables("User::ErrorHandling_PersistentText").Value.ToString
		Dim strLoggingServerName As String = Dts.Variables("$Project::Server_Name_01").Value.ToString
		Dim strLoggingDatabaseName As String = Dts.Variables("$Project::Database_Name_01").Value.ToString
        Dim strTargetServerName As String = Dts.Variables("$Project::Server_Name_02").Value.ToString
        Dim strInitiatingJobName As String = Dts.Variables("$Package::InitiatingJobName").Value.ToString
        Dim strExecutionID As String = Dts.Variables("System::ExecutionInstanceGUID").Value.ToString
		Dim bolFatalError As Boolean = False
		Dim intDatabasesProcessedCount As Integer = CInt(Dts.Variables("User::Databases_ProcessedCount").Value.ToString)
		Dim intDatabasesProcessCompleteCount As Integer = CInt(Dts.Variables("User::Databases_ProcessCompletedCount").Value.ToString)
		Dim intDatabasesProcessFailedCount As Integer = CInt(Dts.Variables("User::ErrorHandling_FailedDatabases_Count").Value.ToString)
		Dim intStatisticsFailedTotalCount As Integer = CInt(Dts.Variables("User::ErrorHandling_FailedStatsUpdate_TotalCount").Value.ToString)
		Dim intDatabasesTotalCount As Integer = CInt(Dts.Variables("User::Databases_TotalCount").Value.ToString)

		Dim bolSendEmail As Boolean = True
		Dim strEmailSource As String = String.Empty
		Dim strPackageCompletionState As String = String.Empty
		Dim strDatabasesErrorText As String = String.Empty
		Dim strJobStatsText As String = String.Empty

		' If others are notified, include SQL Admins in notice of failure.  If no one is notified, at least notify SQL admins
		If Len(Dts.Variables("User::Email_ToLine").Value.ToString) = 0 Then
			Dts.Variables("User::Email_ToLine").Value = "sql_alerts@kyfb.com"
		End If

		strJobStatsText = "Databases--Total: " + intDatabasesTotalCount.ToString + vbCrLf +
		 "Databases--Processed: " + intDatabasesProcessedCount.ToString + vbCrLf +
		 "Databases--Completed: " + intDatabasesProcessCompleteCount.ToString + vbCrLf +
		 "Databases--Failed: " + intDatabasesProcessFailedCount.ToString + vbCrLf +
		"Statistics--Failed Updates: " + intStatisticsFailedTotalCount.ToString + vbCrLf +
		 "Initiating Job Name:" + strInitiatingJobName + vbCrLf +
		 "ExecutionGUID:" + strExecutionID + vbCrLf + vbCrLf

		If (intDatabasesProcessedCount = 0) Or (intDatabasesProcessFailedCount >= intDatabasesProcessCompleteCount) Then
			strDatabasesErrorText = "Access Error:" + vbCrLf + "No databases could be processed or all databases had complete failures updating statistics . This error requires investigation and correction" + vbCrLf +
	"Possible cause could be:" + vbCrLf +
	"--Authority granted to the SQL user: " + strUserName + " is insufficient to the server or database(s)" + vbCrLf +
	"--Database(s) are not in a state allowing for index defragmentation" + vbCrLf +
	"--Errors occurred obtaining index information or performing index defragmentation" + vbCrLf +
	"These are just possible causes and are not the precise reason for the failures."
		Else
			' if there was no fatal error due to the above conditions, there could still be other errors
			If intDatabasesProcessFailedCount = intDatabasesProcessedCount Then
				bolFatalError = True
				strDatabasesErrorText = "Access Error:" + vbCrLf + "No databases could be processed or all databases had complete failures defragmenting indexes.  This error requires investigation and correction" + vbCrLf
			Else
				If intDatabasesProcessFailedCount > 0 Then
					strDatabasesErrorText = "At least one database had problems performing the maintenance."
				Else
					strDatabasesErrorText = "No databases reported issues performing the maintenance."
				End If

			End If
		End If

		If bolFatalError = True Then
			' The package had a fatal error
			strPackageCompletionState = "FAILED--FATAL ERROR"
			bolSendEmail = True
		Else
			' The package did not have a fatal error but may have had non-fatal errors (warnings)
			If intDatabasesProcessFailedCount > 0 Then
				strPackageCompletionState = "COMPLETED--Partial completion or Non-Fatal Errors"
				bolSendEmail = bolSendEmailOverride
			Else
				' If the package did not have a fatal errors and no warnings, then the package completed successfully
				strPackageCompletionState = "COMPLETED--No Errors or Warnings"
				bolSendEmail = bolSendEmailOnSuccess
			End If
		End If

		Dts.Variables("User::Email_Subject").Value = strPackageCompletionState + "--" + Dts.Variables("System::PackageName").Value.ToString
		strEmailSource = "PACKAGE COMPLETION: " + LCase(strPackageCompletionState) + vbCrLf + "Summary and detail logging tables are in the " + strLoggingDatabaseName + " database on the server " + strLoggingServerName + vbCrLf + vbCrLf +
		 "Database Maintenance for databases on: " + strTargetServerName + vbCrLf +
		 strJobStatsText.ToString

		If bolFatalError = True Then
			strEmailSource = strEmailSource + "As the error severity indicated a fatal error, the package experienced issues which prevented any further processing. Corrective action is required." + vbCrLf
		ElseIf intDatabasesProcessFailedCount > 0 Then
			strEmailSource = strEmailSource + "As the error severity indicated a non-fatal error or warning, the package experienced issues but may have further processing. Corrective action may be required." + vbCrLf
		Else
			strEmailSource = strEmailSource + "Please review the summary and details tables for more information." + vbCrLf + strDatabasesErrorText + vbCrLf + "Last Logged Message (additional messagess may be available in the summary table):" + vbCrLf + strPersistentErrorText
		End If

		Dts.Variables("User::Email_Source").Value = Left(Trim(strEmailSource), 4000)
		Dts.Variables("User::ErrorHandling_SendEmail").Value = bolSendEmail

		Dts.TaskResult = ScriptResults.Success
	End Sub

End Class]]></ProjectItem>
