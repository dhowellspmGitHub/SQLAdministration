<System.CLSCompliantAttribute(False)> _
Partial Public Class ScriptMain
    Inherits Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase

    Public Sub Main()
        'User::Database_Name_02_local,User::Schema_Name,User::Statistic_Name,User::Table_Name
        ' User::StatsDetail_Comments,User::StatsDetail_Scan_Type_Performed,User::StatsDetail_UpdateEndDate,User::StatsDetail_UpdateFailedInd,User::StatsDetail_UpdateFromIndexDefrag,User::StatsDetail_UpdatePerformed_Ind,User::StatsDetail_UpdateStartDate
        Dim strDatabaseName As String = Dts.Variables("User::Database_Name_02_local").Value.ToString
        Dim strSchemaName As String = Dts.Variables("User::Schema_Name").Value.ToString
        Dim strTableName As String = Dts.Variables("User::Table_Name").Value.ToString
        Dim strStatsName As String = Dts.Variables("User::Statistic_Name").Value.ToString
        Dim strObjectName As String = "[" + strDatabaseName + "].[" + strSchemaName + "].[" + strTableName + "] Statistic:" + strStatsName

        Dim strDetailComments As String = Dts.Variables("User::StatsDetail_Comments").Value.ToString
        strDetailComments = "FAILED: Review Statistics Detail table for more information;" + strDetailComments
        Dim strSubComponent As String = "STATISTICS UPDATE FAILED:" + strObjectName
        Dts.Variables("User::StatsDetail_Scan_Type_Performed").Value = 0

        Dts.Variables("User::StatsDetail_UpdateEndDate").Value = Now()
        Dts.Variables("User::StatsDetail_UpdateFailedInd").Value = True
        Dts.Variables("User::StatsDetail_UpdateFromIndexDefrag").Value = False
        Dts.Variables("User::StatsDetail_UpdatePerformed_Ind").Value = False
        Dts.Variables("User::StatsDetail_Comments").Value = strDetailComments
        Dts.Events.FireError(-1, strSubComponent, strDetailComments, String.Empty, 0)


        Dts.TaskResult = ScriptResults.Success
    End Sub

#Region "ScriptResults declaration"
    'This enum provides a convenient shorthand within the scope of this class for setting the
    'result of the script.

    'This code was generated automatically.
    Enum ScriptResults
        Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
