        ' RW Variables: User::Statistics_Processed_Count,User::Statistics_TotalCount,User::StatsSummary_Comments,User::StatsSummary_Performed_Ind,User::StatsSummary_Process_Complete_Ind


        'Dim strSourceName As String = Dts.Variables("System::SourceName").Value.ToString
        Dim strSourceName As String = "Get Statistics DFT"
        Dim strLocalErrorMessage As String
        strLocalErrorMessage = "ERROR at task: " + strSourceName + ": Could not load the statistics data for this database to dataset. Please confirm the password and authority granted to the user:" + Dts.Variables("$Project::Login_Name_02").Value.ToString + " and that the database is online and in a writable state"
