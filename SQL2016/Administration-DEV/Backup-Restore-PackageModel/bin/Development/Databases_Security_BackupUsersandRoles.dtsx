            Name="ScriptMain.vb"
            Encoding="UTF8"><![CDATA[' Microsoft SQL Server Integration Services Script Task
' Write scripts using Microsoft Visual Basic 2008.
' The ScriptMain is the entry point class of the script.

Imports System
Imports System.Data
Imports System.Math
Imports Microsoft.SqlServer.Dts.Runtime
Imports Microsoft.SqlServer.Management.Smo
Imports Microsoft.SqlServer
Imports Microsoft.SqlServer.Dts.Runtime.ConnectionInfo
Imports System.IO


<Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute> _
<System.CLSCompliantAttribute(False)> _
Partial Public Class ScriptMain
	Inherits Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase

	Enum ScriptResults
		Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
		Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
	End Enum
	
    Public Sub Main()
        ' User::Database_Name_02,User::Email_Task_Results,User::Server_Name_01

        Dim objServer As Microsoft.SqlServer.Management.Smo.Server
        Dim objDatabase As Database
        Dim strServerName As String = Dts.Variables("User::Server_Name_01").Value.ToString
        Dim strDatabaseName As String = Dts.Variables("User::Database_Name_02").Value.ToString
        Dim strEventLogDesc As String = String.Empty
        Dim objTaskResults As Dts.Runtime.DTSExecResult = DTSExecResult.Success
        Dim strEventSubComp As String = Dts.Variables("System::TaskName").Value.ToString
        Dim strTaskResults As String = Dts.Variables("User::ErrorHandling_TaskResults").Value.ToString
        Dts.Variables("User::ErrorHandling_PackageFailed").Value = False

        Try
            objServer = DirectCast(Dts.Connections("SMO.Server_02").AcquireConnection(Dts.Transaction), Microsoft.SqlServer.Management.Smo.Server)
            If objServer.Databases.Contains(strDatabaseName) Then
                objDatabase = objServer.Databases(strDatabaseName)
                objServer.KillAllProcesses(strDatabaseName)
                strTaskResults = strTaskResults & vbCrLf & "COMPLETED: All processes killed for database " & strDatabaseName & " on server " & strServerName
                objDatabase.Drop()
                strTaskResults = strTaskResults & vbCrLf & "COMPLETED: Database " & strDatabaseName & " dropped on server " & strServerName
            Else
                ' Database does not exist--fail package
                strEventLogDesc = "ERROR at " & strEventSubComp & ": Database " & strDatabaseName & " does not exists on server " & strServerName
                Throw New Exception(strEventLogDesc)
                objTaskResults = DTSExecResult.Failure
            End If

        Catch smoex As SmoException
            'Display the sequence of non-SMO exceptions that caused the SMO exception.
            Dim strInnerMsg As String = String.Empty
            Dim exi As Exception
            exi = smoex.InnerException
            Do While exi.InnerException IsNot (Nothing)
                strInnerMsg = strInnerMsg & exi.InnerException.Message.ToString
                exi = exi.InnerException
            Loop
            strEventLogDesc = exi.Message & vbTab & strInnerMsg
            strTaskResults = strTaskResults & strEventLogDesc
            Dts.Variables("User::ErrorHandling_PackageFailed").Value = True
            Dts.Events.FireError(0, strEventSubComp, strEventLogDesc, String.Empty, 0)

            'Catch other non-SMO exceptions.
        Catch ex As Exception
            'Exception handling 
            Dim strInnerMsg As String = "Inner Exception:"
            Do While ex.InnerException IsNot Nothing
                strInnerMsg = strInnerMsg & ex.InnerException.Message.ToString
                ex = ex.InnerException
            Loop
            strEventLogDesc = ex.Message & vbTab & strInnerMsg
            Dts.Variables("User::ErrorHandling_PackageFailed").Value = True
            strTaskResults = strTaskResults & strEventLogDesc
        Finally
            Dts.Variables("User::ErrorHandling_TaskResults").Value = strTaskResults
        End Try

        Dts.TaskResult = objTaskResults
    End Sub

End Class]]></ProjectItem>
