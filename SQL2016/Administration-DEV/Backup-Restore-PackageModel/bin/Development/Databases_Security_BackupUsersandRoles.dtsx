            <component
              refId="Package\DFT Record Restore database access to table\ADO SRC CustomRole Permissions"
              componentClassID="Microsoft.ManagedComponentHost"
              contactInfo="Extracts data from a relational database by using a .NET provider.;Microsoft Corporation; Microsoft SqlServer v10; © 2007 Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;4"
              description="Extracts data from a relational database by using a .NET provider."
              name="ADO SRC CustomRole Permissions"
              usesDispositions="true"
              version="4">
              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement used by the component to extract data."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
DB_NAME() AS databasename,
DP2.class,
DP2.class_desc,
DP2.major_id,
DP2.minor_id,
ISNULL(SCHEMA_NAME(so1.schema_id),'dbo') AS [SCHEMA_NAME],
SO1.name,
so1.type_desc,
DP2.grantee_principal_id,
DP2.[type],
DP2.permission_name,
DP2.[state],
DP2.[state_desc]
FROM sys.database_permissions AS DP2
INNER JOIN sys.database_principals AS DP1
ON DP2.grantee_principal_id = DP1.principal_id
LEFT OUTER JOIN sys.objects AS SO1
ON DP2.major_id = so1.[object_id]
WHERE dp1.type = 'R'
AND dp1.is_fixed_role &lt;&gt; 1
AND dp1.principal_id &gt; 0
AND [dp1].[name] NOT IN ('dbo','sys','guest','INFORMATION_SCHEMA')</property>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">30</property>
