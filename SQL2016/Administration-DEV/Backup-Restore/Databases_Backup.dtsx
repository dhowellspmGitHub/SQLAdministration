#Region "Imports"
Imports System
Imports System.Data
Imports System.Math
Imports Microsoft.SqlServer.Dts.Runtime
#End Region

'ScriptMain is the entry point class of the script.  Do not change the name, attributes,
'or parent of this class.
<Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute()> _
<System.CLSCompliantAttribute(False)> _
Partial Public Class ScriptMain
    Inherits Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase

    Public Sub Main()
        ' RW Vars: User::Backup_Stats_Backup_Completed,User::Backup_Stats_Backup_Failed,User::Backup_Stats_Database_Processed,User::Backup_Stats_Detail_Comments,User::Backup_Stats_TaskEndTime,User::Databases_ProcessedCount

        ' Since the prior task performing the FULL was successful, set the processing variables accordingly
        ' If the prior task complete successfully, then set the backup detail completed flag = true
        ' This cannot be accomplished in the PostExecute task
        ' RW Vars: User::Backup_Stats_Backup_Completed
        Dim strTaskName As String = Dts.Variables("System::SourceName").Value.ToString
        Dts.Variables("User::Backup_Stats_Backup_Completed").Value = True
        Dts.Variables("User::Backup_Stats_Backup_Failed").Value = False
        Dts.Variables("User::Backup_Stats_Database_Processed").Value = True

        Dts.Variables("User::Backup_Stats_TaskEndTime").Value = Now()
        Dts.Variables("User::Databases_ProcessedCount").Value = CInt(Dts.Variables("User::Databases_ProcessedCount").Value.ToString) + 1

        Dim strBackupDetailComments As String = Dts.Variables("User::Backup_Stats_Detail_Comments").Value.ToString

        Dim strTaskMessage As String = "FULL OVERRIDE-backup completed successfully; "
        Dts.Events.FireInformation(0, strTaskName, strTaskMessage, String.Empty, 0, False)
        Dts.Variables("User::Backup_Stats_Detail_Comments").Value = strBackupDetailComments + strTaskMessage

        Dts.TaskResult = ScriptResults.Success
    End Sub

#Region "ScriptResults declaration"
    'This enum provides a convenient shorthand within the scope of this class for setting the
    'result of the script.

    'This code was generated automatically.
    Enum ScriptResults
        Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
        Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
    End Enum

#End Region

