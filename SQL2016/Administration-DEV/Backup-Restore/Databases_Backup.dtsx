                Encoding="UTF8"><![CDATA[#Region "Imports"
Imports System
Imports System.Data
Imports System.Math
Imports Microsoft.SqlServer.Dts.Runtime
#End Region

'ScriptMain is the entry point class of the script.  Do not change the name, attributes,
'or parent of this class.
<Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute()> _
<System.CLSCompliantAttribute(False)> _
Partial Public Class ScriptMain
    Inherits Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase

    ' Before the detail history table is updated and the backup atttempted, reset the process indicators and other package variables
    ' RW Vars: User::Backup_Stats_Backup_Completed,User::Backup_Stats_Backup_Failed,User::Backup_Stats_Database_Accessible_Ind,User::Backup_Stats_Database_Excluded_State_Ind,User::Backup_Stats_Database_Processed,User::Backup_Stats_Database_Status,User::Backup_Stats_PackageStartTime,User::Backup_Stats_PrimaryPathUsed


    Public Sub Main()
        Dts.Variables("User::Backup_Stats_Database_Processed").Value = False
        Dts.Variables("User::Backup_Stats_Backup_Completed").Value = False
        Dts.Variables("User::Backup_Stats_Backup_Failed").Value = False
        Dts.Variables("User::Backup_Stats_Database_Accessible_Ind").Value = True
        Dts.Variables("User::Backup_Stats_Database_Excluded_State_Ind").Value = False
        Dts.Variables("User::Backup_Stats_Database_Status").Value = String.Empty
        Dts.Variables("User::Backup_Stats_PrimaryPathUsed").Value = True
        Dts.Variables("User::Backup_Stats_PackageStartTime").Value = Now()


        Dts.TaskResult = ScriptResults.Success
    End Sub

#Region "ScriptResults declaration"
    'This enum provides a convenient shorthand within the scope of this class for setting the
    'result of the script.

    'This code was generated automatically.
    Enum ScriptResults
        Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
        Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
    End Enum

#End Region

End Class]]></ProjectItem>
