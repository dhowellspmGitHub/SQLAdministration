    Public Sub Main()
        ' RO Vars: System::SourceName,User::Backup_History_SQLBackupFile
        ' RW Vars: User::ErrorHandling_FileDeletionErrors,User::ErrorHandling_FileDeletionFailures
        Dim intFileDeletionErrors As Integer = CInt(Dts.Variables("User::ErrorHandling_FileDeletionFailures").Value.ToString) + 1
        Dim strBackupFileName As String = Dts.Variables("User::Backup_History_SQLBackupFile").Value.ToString
        Dim strFileDeletionErrors As String = Dts.Variables("User::ErrorHandling_FileDeletionErrors").Value.ToString + vbCrLf + strBackupFileName
        Dim strEventMessage As String = strBackupFileName + "--could not be found or deleted. Manually review if file exists and delete"
        Dts.Variables("User::ErrorHandling_FileDeletionErrors").Value = strFileDeletionErrors
        Dts.Variables("User::ErrorHandling_FileDeletionFailures").Value = intFileDeletionErrors

        Dts.Events.FireWarning(0, Dts.Variables("System::SourceName").Value.ToString, strEventMessage, String.Empty, 0)
        Dts.TaskResult = ScriptResults.Success
    End Sub

