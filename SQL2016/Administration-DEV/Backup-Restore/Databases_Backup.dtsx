                  <ProjectItem
                    Name="ScriptMain.vb"
                    Encoding="UTF8"><![CDATA[#Region "Imports"
Imports System
Imports System.Data
Imports System.Math
Imports Microsoft.SqlServer.Dts.Runtime
#End Region

'ScriptMain is the entry point class of the script.  Do not change the name, attributes,
'or parent of this class.
<Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute()> _
<System.CLSCompliantAttribute(False)> _
Partial Public Class ScriptMain
    Inherits Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase

#Region "ScriptResults declaration"
    'This enum provides a convenient shorthand within the scope of this class for setting the
    'result of the script.

    'This code was generated automatically.
    Enum ScriptResults
        Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
        Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
    End Enum

#End Region

    Public Sub Main()
        ' Script Notes, Information and Description:
        ' First, confirm the database to backup is in a valid state and is not a SQL snapshot
        'Next, determine --compression options must be specified as a string in the compression options variable if the compression options for DDBoost are available (and if so specified in the compression options)--If using a third party compression option (such as EMC DDBoost or other method), those methods can only be used by this package if their software includes capability to use the feature in TSQL commands.  compression option other than standard SQL compression, which can be specified via TSQL, those thir
        ' Backup types (basically 4--FULL, DIFFERENTIAL, LOG and COPY_ONLY (which is a full backup but does not mark the transaction log nor truncate it)

        ' Input package variables
        ' RO Vars: System::TaskName,User::Backup_Parameters_CompressionOptions,User::Backup_Parameters_IncludeCompression,User::Backup_Parameters_PeformCopyOnly,User::Backup_Parameters_PerformDifferential,User::Backup_Stats_Backup_FullPathName,User::Database_Name_02_local,$Package::InitiatingJobName,$Package::InitiatingJobScheduler,$Project::Server_Name_02

        ' RW Vars: User::Backup_Stats_Detail_Comments,User::Backup_Stats_TaskEndTime,User::Backup_Stats_TaskStartTime,User::Backup_TSQLCommand


        Dim strTaskName As String = Dts.Variables("System::TaskName").Value.ToString
        Dim strCompressionOptions As String = Dts.Variables("User::Backup_Parameters_CompressionOptions").Value.ToString
        Dim bolCompressionOptionInd As Boolean = CBool(Dts.Variables("User::Backup_Parameters_IncludeCompression").Value.ToString)
        Dim bolCopyOnlyOptionInd As Boolean = CBool(Dts.Variables("User::Backup_Parameters_PeformCopyOnly").Value.ToString)
        Dim bolPerformDiffBackupInd As Boolean = CBool(Dts.Variables("User::Backup_Parameters_PerformDifferential").Value.ToString)
        Dim strFullBackupPath As String = Dts.Variables("User::Backup_Stats_Backup_FullPathName").Value.ToString
        Dim strDatabaseName As String = Dts.Variables("User::Database_Name_02_local").Value.ToString
        Dim strBackupDetailComments As String = Dts.Variables("User::Backup_Stats_Detail_Comments").Value.ToString

        Dim strInitiatingJobName As String = Dts.Variables("$Package::InitiatingJobName").Value.ToString
        Dim strInitiatingJobScheduler As String = Dts.Variables("$Package::InitiatingJobScheduler").Value.ToString

        'error handling
        Dim bolDTSTaskResults As DTSExecResult = DTSExecResult.Success
        Dim strExceptionMessage As String
        Dim strTaskMessage As String = String.Empty

        ' Backup values and options
        Dim strBackupName As String
        Dim strBackupTSQLToRun As String = String.Empty
        Dim strBackupType As String
        Dim strCompressionString As String = String.Empty
        Dim dtmBackupTimeStamp As Date = Now()
        Dim strBackupTimeStamp As String = "NoTimeStamp"

        Try
            ' With the removal of all SMO code, no database state validation is performed any longer in the script task
            ' The status and availability of each database is performed in the task loading database names to the Databases_Backup_History_Detail table.  Only databases in an online and available state are selected into the Databases_List variable from which the FELC gets its name.
            ' There is assumed no change in database availability between the initial load of that table and the attempt to build the TSQL backup command.  However, if backups take a long time to complete, it is possible the database has changed its state during the backup of another database.  This will appear as package failures when the SQL Task is attempted
            ' Cannot do a copy only backup if performing a differential
            If bolPerformDiffBackupInd = True Then
                bolCopyOnlyOptionInd = False
                strBackupType = "DIFFERENTIAL,"
                strBackupName = strDatabaseName + "-" + "Differential Database Backup--Performed via " + strInitiatingJobName
                ' Need to add validation here for differential
                ' If the backup history shows there are no FULL backups or the history shows a full backup but the backup has been deleted, then the differential cannot be performed.  Therefore, force to do a full backup

            Else
                strBackupType = String.Empty
                strBackupName = strDatabaseName + "-" + "Full Database Backup--Performed via " + strInitiatingJobName + "--Job Scheduler:" + strInitiatingJobScheduler
            End If

            If bolCopyOnlyOptionInd = True Then
                strBackupType = "COPY_ONLY,"
                strBackupName = strDatabaseName + "-" + "Copy Only Database Backup--Performed via " + strInitiatingJobName + "--Job Scheduler:" + strInitiatingJobScheduler
            End If
            If bolCompressionOptionInd = True Then
                strCompressionString = ",COMPRESSION"
            End If
            strBackupTimeStamp = CStr(DatePart(DateInterval.Year, dtmBackupTimeStamp)) + Right("00" + CStr(DatePart(DateInterval.Month, dtmBackupTimeStamp)), 2) + Right("00" + CStr(DatePart(DateInterval.Day, dtmBackupTimeStamp)), 2) + Right("00" + CStr(DatePart(DateInterval.Hour, dtmBackupTimeStamp)), 2) + Right("00" + CStr(DatePart(DateInterval.Minute, dtmBackupTimeStamp)), 2)
            strBackupName = strDatabaseName + "-Backup via " + strInitiatingJobName + "--Job Scheduler:" + strInitiatingJobScheduler
            strFullBackupPath = strFullBackupPath + "\" + strDatabaseName + "_" + strBackupTimeStamp + ".bak"

            ' Set the TSQL command to run from the variables
            strBackupTSQLToRun = "BACKUP DATABASE [" + strDatabaseName + "] TO  DISK = N'" + strFullBackupPath + "' WITH " +
                        strBackupType + " NOFORMAT, INIT, NAME = '" + strBackupName + "', SKIP, NOREWIND, NOUNLOAD" + strCompressionString
            strTaskMessage = "Database backup script built: Database exists and proceeding to backup task; "
            Dts.Events.FireInformation(0, strTaskName, strTaskMessage, String.Empty, 0, False)

        Catch ex As Exception
            Dim strInnerMsg As String = "Inner Exception:"
            Do While ex.InnerException IsNot Nothing
                strInnerMsg = strInnerMsg & ex.InnerException.Message.ToString
                ex = ex.InnerException
            Loop
            strExceptionMessage = ex.Message + vbTab + strInnerMsg
            strTaskMessage = "OPERATION FAILED: " + strExceptionMessage + "; "
            Dts.Events.FireError(0, strTaskName, strExceptionMessage, String.Empty, 0)
            bolDTSTaskResults = DTSExecResult.Failure


        Finally
            Dts.Variables("User::Backup_Stats_Detail_Comments").Value = strBackupDetailComments + strTaskMessage
            Dts.Variables("User::Backup_Stats_TaskEndTime").Value = Now()
            Dts.Variables("User::Backup_Stats_TaskStartTime").Value = dtmBackupTimeStamp
            Dts.Variables("User::Backup_TSQLCommand").Value = strBackupTSQLToRun
        End Try
        Dts.TaskResult = bolDTSTaskResults
    End Sub

End Class]]></ProjectItem>
