        ' RO Vars: User::Backup_Stats_CountCompleted,User::Backup_Stats_CountProcessed,User::Backup_Stats_CountSkipped,User::ErrorHandling_FailedDatabases_Count,User::ErrorHandling_FatalErrors_Count,User::ErrorHandling_FileDeletionErrors,User::ErrorHandling_FileDeletionFailures,User::ErrorHandling_NonFatalErrors_Count,User::ErrorHandling_PersistentText,User::ErrorHandling_SendEmailOnSuccess,System::ExecutionInstanceGUID,System::PackageName,$Package::ErrorHandling_SendEmail,$Package::InitiatingJobName,$Package::InitiatingJobScheduler,$Project::Database_Name_01,$Project::Login_Name_02,$Project::Server_Name_01,$Project::Server_Name_02

        ' RW Vars: User::Email_Source,User::Email_Subject,User::Email_ToLine,User::ErrorHandling_SendEmail

        Dim intDatabasesCountCompleted As Integer = CInt(Dts.Variables("User::Backup_Stats_CountCompleted").Value.ToString)
        Dim intDatabasesCountProcessed As Integer = CInt(Dts.Variables("User::Backup_Stats_CountProcessed").Value.ToString)
        Dim intDatabasesCountSkipped As Integer = CInt(Dts.Variables("User::Backup_Stats_CountSkipped").Value.ToString)
        Dim intDatabasesCountFailed As Integer = CInt(Dts.Variables("User::ErrorHandling_FailedDatabases_Count").Value.ToString)
        Dim intRunningFECount As Integer = CInt(Dts.Variables("User::ErrorHandling_FatalErrors_Count").Value.ToString)
        Dim strFileDeletionFailures As String = Dts.Variables("User::ErrorHandling_FileDeletionErrors").Value.ToString
        Dim intFileDeletionFailures As Integer = CInt(Dts.Variables("User::ErrorHandling_FileDeletionFailures").Value.ToString)
        Dim intRunningNFECount As Integer = CInt(Dts.Variables("User::ErrorHandling_NonFatalErrors_Count").Value.ToString)
        Dim strPersistentErrorText As String = Dts.Variables("User::ErrorHandling_PersistentText").Value.ToString
        Dim bolSendEmailOnSuccess As Boolean = CBool(Dts.Variables("User::ErrorHandling_SendEmailOnSuccess").Value.ToString)
        Dim strExecutionID As String = Dts.Variables("System::ExecutionInstanceGUID").Value.ToString
        Dim strPackageName As String = Dts.Variables("System::PackageName").Value.ToString
        Dim bolSendEmailOverride As Boolean = CBool(Dts.Variables("$Package::ErrorHandling_SendEmail").Value.ToString)
        Dim strInitiatingJobName As String = Dts.Variables("$Package::InitiatingJobName").Value.ToString
        Dim strInitiatingJobScheduler As String = Dts.Variables("$Package::InitiatingJobScheduler").Value.ToString
        Dim strLoggingDatabaseName As String = Dts.Variables("$Project::Database_Name_01").Value.ToString
        Dim strUserName As String = Dts.Variables("$Project::Login_Name_02").Value.ToString
        Dim strLoggingServerName As String = Dts.Variables("$Project::Server_Name_01").Value.ToString
        Dim strTargetServerName As String = Dts.Variables("$Project::Server_Name_02").Value.ToString

        Dim strEmailToLine As String = Dts.Variables("User::Email_ToLine").Value.ToString
        Dim intDatabasesCountTotal As Integer
        Dim bolSendEmail As Boolean = True
        Dim strEmailSource As String = String.Empty
        Dim strPackageCompletionState As String = String.Empty
        Dim strDatabasesErrorText As String = String.Empty
        Dim strJobStatsText As String = String.Empty

        ' If others are notified, include SQL Admins in notice of failure.  If no one is notified, at least notify SQL admins
        If Len(strEmailToLine) = 0 Then
            Dts.Variables("User::Email_ToLine").Value = "sql_alerts@kyfb.com"

        End If

        intDatabasesCountTotal = intDatabasesCountProcessed + intDatabasesCountSkipped
        strJobStatsText = "Initiating Job Name:" + strInitiatingJobName + vbCrLf +
            "Initiating Job Scheduler:" + strInitiatingJobScheduler + vbCrLf +
            "Backup Job Database Counts:" + vbCrLf +
            "Databases--Total: " + intDatabasesCountTotal.ToString + vbCrLf +
            "Databases--Processed: " + intDatabasesCountProcessed.ToString + vbCrLf +
            "Databases--Completed: " + intDatabasesCountCompleted.ToString + vbCrLf +
            "Databases--Skipped: " + intDatabasesCountSkipped.ToString + vbCrLf +
            "Databases--Failed: " + intDatabasesCountFailed.ToString + vbCrLf +
            "Fatal error Count: " + intRunningFECount.ToString + vbCrLf +
            "Non fatal error count: " + intRunningNFECount.ToString + vbCrLf +
            "SSIS ExecutionGUID:" + strExecutionID + vbCrLf + vbCrLf


        If (intDatabasesCountCompleted = 0) Then
            intRunningFECount = intRunningFECount + 1
            strDatabasesErrorText = "Access Error:" + vbCrLf + "No databases could be processed or all databases had complete failures.  This error requires investigation and correction" + vbCrLf +
             "Possible cause could be:" + vbCrLf +
             "--Authority granted to the SQL user: " + strUserName + " is insufficient to the server or database(s)" + vbCrLf +
             "--Database(s) are not in a online and available state " + vbCrLf +
             "These are just possible causes and are not the precise reason for the failures."
        Else
            ' if there was no fatal error due to the above conditions, there could still be other errors
            If intDatabasesCountFailed > 0 Then
                strDatabasesErrorText = "Access Error:" + vbCrLf + "Some databases could be processed or all databases had complete failures.  This error requires investigation and correction" + vbCrLf
            Else
                If intRunningNFECount > 0 Then
                    strDatabasesErrorText = "At least one database had problems."
                Else
                    strDatabasesErrorText = "No databases reported issues."
                End If

            End If
        End If

        If intRunningFECount > 0 Then
            ' The package had a fatal error
            strPackageCompletionState = "FAILED--FATAL ERROR"
            bolSendEmail = True
        Else
            ' The package did not have a fatal error but may have had non-fatal errors (warnings)
            If intRunningNFECount > 0 Then
                strPackageCompletionState = "COMPLETED--Partial completion or Non-Fatal Errors"
                bolSendEmail = bolSendEmailOverride
            Else
                ' If the package did not have a fatal errors and no warnings, then the package completed successfully
                strPackageCompletionState = "COMPLETED--No Errors or Warnings"
                bolSendEmail = bolSendEmailOnSuccess
            End If
        End If

        Dts.Variables("User::Email_Subject").Value = strPackageCompletionState + "--" + strPackageName
        strEmailSource = "At least one task in the package " + LCase(strPackageCompletionState) + vbCrLf + "Summary and detail logging tables are in the " + strLoggingDatabaseName + " database on the server " + strLoggingServerName + vbCrLf + vbCrLf +
         "Database Backups on: " + strTargetServerName + vbCrLf +
         strJobStatsText.ToString

        If intRunningFECount > 0 Then
            strEmailSource = strEmailSource + "As the error severity indicated a fatal error, the package experienced issues which prevented any further processing. Corrective action is required." + vbCrLf
        Else
            strEmailSource = strEmailSource + "As the error severity indicated a non-fatal error or warning, the package experienced issues but may have further processing. Corrective action may be required." + vbCrLf
            If intFileDeletionFailures > 0 Then
                strEmailSource = strEmailSource + "Errors occurred deleting SQL backup files. The package could not delete " + intFileDeletionFailures.ToString + " number of backup files. Please review the list below for any backup files to manually delete." + vbCrLf
            End If
        End If
        strEmailSource = strEmailSource + "Please review the summary and details tables for more information." + vbCrLf + strDatabasesErrorText + vbCrLf + "Last Logged Message (additional messagess may be available in the summary table):" + vbCrLf + strPersistentErrorText + vbCrLf + "List of SQL File Backups to investigate:" + vbCrLf + StrDup(40, "-") + vbCrLf + strFileDeletionFailures.ToString


        Dts.Variables("User::Email_Source").Value = Left(Trim(strEmailSource), 4000)
        Dts.Variables("User::ErrorHandling_SendEmail").Value = bolSendEmail

        Dts.TaskResult = ScriptResults.Success
    End Sub

#Region "ScriptResults declaration"
    'This enum provides a convenient shorthand within the scope of this class for setting the
    'result of the script.

    'This code was generated automatically.
    Enum ScriptResults
        Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
        Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
    End Enum

#End Region

