                            Encoding="UTF8"><![CDATA[
#Region "Imports"
Imports System
Imports System.Data
Imports System.Math
Imports Microsoft.SqlServer.Dts.Runtime
#End Region

'ScriptMain is the entry point class of the script.  Do not change the name, attributes,
'or parent of this class.
<Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute()> _
<System.CLSCompliantAttribute(False)> _
Partial Public Class ScriptMain
    Inherits Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase

    'This method is called when this script task executes in the control flow.
    'Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
    'To open Help, press F1.

    Public Sub Main()
        '
        ' Add your code here
        ' RO Vars: System::ErrorCode,System::ErrorDescription,System::SourceName,User::Backup_Parameters_PerformDifferential,User::Backup_Stats_SecondaryPathUsed

        ' RW Vars: System::ErrorCode,System::ErrorDescription,System::SourceName,User::Backup_Parameters_PerformDifferential,User::Backup_Stats_PrimaryPathUsed

        ' DESCRIPTION: This script evaluates the error message recorded by the SSIS processing logs. If the error text indicates the backup attempt failed because a differential backup was attempted and there is no FULL backup on which the DIFFERENTIAL could be based, then it alters the TSQL command text to attempt a FULL backup instead.  This is the only condition on which later tasks in this ON ERROR condition run.      ' 

        Dim strCurrentErrorDescription As String = Dts.Variables("System::ErrorDescription").Value.ToString
        Dim strTaskName As String = Dts.Variables("System::SourceName").Value.ToString
        Dim bolPerformDiffBackupInd As Boolean = CBool(Dts.Variables("User::Backup_Parameters_PerformDifferential").Value.ToString)
        Dim bolPrimaryPathUsed As Boolean = CBool(Dts.Variables("User::Backup_Stats_PrimaryPathUsed").Value.ToString)

        Dim strBackupDetailComments As String = Dts.Variables("User::Backup_Stats_Detail_Comments").Value.ToString
        Dim strBackupTSQLToRun As String = Dts.Variables("User::Backup_TSQLCommand").Value.ToString
        Dim intRunningFailedDatabasesCount As Integer = CInt(Dts.Variables("User::ErrorHandling_FailedDatabases_Count").Value.ToString)

        Dim bolDTSTaskResults As DTSExecResult = DTSExecResult.Success
        Dim strExceptionMessage As String = String.Empty
        Dim strTaskMessage As String = String.Empty
        ' As this is an "ON ERROR" scenario, the default values for the script assume the backup failed for a reason other than a non-existent full backup
        Dim bolBackupFailedInd As Boolean = True
        Dim bolBackupCompletedInd As Boolean = False
        Dim bolDatabaseProcessedInd As Boolean = False

        Dim strPerformFullBackupErrorString As String = "Perform a full database backup by reissuing"

        Try
            ' The ON ERROR process will only continue if a) a differential backup was attempted AND b) the SSIS package logs contain specific text symptomatic of the condition (that there is no FULL backup on which to base the DIFFERENTIAL).
            ' Change the backup type variables and TSQL string replacing the WITH DIFFERENTIAL with an empty string
            If (strCurrentErrorDescription.Contains(strPerformFullBackupErrorString) = True) And (bolPerformDiffBackupInd = True) Then
                ' If both are true, then the best effort is to attempt a FULL backup

                strBackupTSQLToRun = strBackupTSQLToRun.Replace("DIFFERENTIAL,", String.Empty)
                strTaskMessage = "OVERRIDE OF DIFFERENTIAL: Error message 'Cannot perform a differential backup for database because a current database backup does not exist. Attempting a full database backup by replacing the DIFFERENTIAL option and reissuing the TSQL Command variable; "

                ' Reset the processing, completed and failed indicators for this database if retrying the backup
                bolPerformDiffBackupInd = False
                bolBackupCompletedInd = False
                bolBackupFailedInd = False
            Else
                ' The backup was not a differential or the backup failed for other reasons.  
                ' Set this backup attempt as failed and other settings appropriately
                strTaskMessage = "SQL ERROR MESSAGES: " + strCurrentErrorDescription.ToString + "; "
                Throw New Exception(strTaskMessage)
            End If

        Catch ex As Exception
            Dim strInnerMsg As String = "Inner Exception:"
            Do While ex.InnerException IsNot Nothing
                strInnerMsg = strInnerMsg & ex.InnerException.Message.ToString
                ex = ex.InnerException
            Loop
            If bolPrimaryPathUsed = False Then
                intRunningFailedDatabasesCount = intRunningFailedDatabasesCount + 1
            Else
                intRunningFailedDatabasesCount = intRunningFailedDatabasesCount
            End If
            Dts.Variables("User::Backup_Stats_TaskEndTime").Value = Now()
            strExceptionMessage = ex.Message + vbTab + strInnerMsg
            strExceptionMessage = strInnerMsg
            strTaskMessage = "OPERATION FAILED: " + strExceptionMessage + "; "
            Dts.Events.FireWarning(0, strTaskName, strExceptionMessage, String.Empty, 0)
            'Dts.Events.FireError(0, strTaskName, strExceptionMessage, String.Empty, 0)
        Finally
            ' RW Vars: User::Backup_Stats_Backup_Completed,User::Backup_Stats_Backup_Failed,User::Backup_Stats_Database_Processed,User::Backup_Stats_Detail_Comments,User::Backup_Stats_TaskEndTime,User::Backup_TSQLCommand,User::ErrorHandling_FailedDatabases_Count
            Dts.Variables("User::Backup_Stats_Backup_Completed").Value = bolBackupCompletedInd
            Dts.Variables("User::Backup_Stats_Backup_Failed").Value = bolBackupFailedInd
            Dts.Variables("User::Backup_Stats_Database_Processed").Value = bolDatabaseProcessedInd
            Dts.Variables("User::Backup_Stats_Detail_Comments").Value = strBackupDetailComments + strTaskMessage
            Dts.Variables("User::Backup_TSQLCommand").Value = strBackupTSQLToRun
            Dts.Variables("User::ErrorHandling_FailedDatabases_Count").Value = intRunningFailedDatabasesCount
        End Try


        Dts.TaskResult = ScriptResults.Success
    End Sub

#Region "ScriptResults declaration"
    'This enum provides a convenient shorthand within the scope of this class for setting the
    'result of the script.

    'This code was generated automatically.
    Enum ScriptResults
        Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
        Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
    End Enum

#End Region

End Class]]></ProjectItem>
