          <ProjectItem
            Name="ScriptMain.vb"
            Encoding="UTF8"><![CDATA[' Microsoft SQL Server Integration Services Script Task
' Write scripts using Microsoft Visual Basic 2008.
' The ScriptMain is the entry point class of the script.

Imports System
Imports System.Data
Imports System.Math
Imports Microsoft.SqlServer.Dts.Runtime

<Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute> _
<System.CLSCompliantAttribute(False)> _
Partial Public Class ScriptMain
	Inherits Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase

	Enum ScriptResults
		Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
		Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
	End Enum
    Public Sub Main()
        'System::PackageName
        'User::Database_ExistsInd,User::ErrorHandling_CompletedDatabases_Count,User::ErrorHandling_FailedDatabases_Count,User::ErrorHandling_TotalDatabases_Count,System::PackageName
        ' RW: User::Backup_Parameters_Databases_ToProcess,User::Backup_Parameters_Process_All_Databases,User::Database_Name_01,User::Database_Name_02,User::ErrorHandling_Email_CC,User::ErrorHandling_Email_Message,User::ErrorHandling_Email_Subject,User::ErrorHandling_Email_ToLine,User::ErrorHandling_PackageFailed,User::ErrorHandling_SendEmailOnSuccess,User::ErrorHandling_TaskResults,User::Server_Name_01,User::Server_Name_02
        ' This script is to complete the build of text fields and set notification variables used Email--Send completion status task
        Dim strServerName As String = Dts.Variables("User::Server_Name_02").Value.ToString
        Dim strDatabasesParameters As String = Dts.Variables("User::Backup_Parameters_Databases_ToProcess").Value.ToString
        Dim strDatabasesParameters2 As String = Dts.Variables("User::Backup_Parameters_Process_All_Databases").Value.ToString

        Dim intFailedDBCount As Integer = CInt(Dts.Variables("User::ErrorHandling_FailedDatabases_Count").Value.ToString)
        Dim intCompletedDBCount As Integer = CInt(Dts.Variables("User::ErrorHandling_CompletedDatabases_Count").Value.ToString)
        Dim intTotalDBCount As Integer = CInt(Dts.Variables("User::ErrorHandling_TotalDatabases_Count").Value.ToString)
        Dim strEmailTaskOutcomes As String
        Dim strEmailSubject As String
        Dim strEmailMessage As String = String.Empty
        Dim strEmailCC As String
        Dim strEmailMessageFooter As String = "Please review the summary table and/or package logs output for more information"

        Dim strPackagName As String = Dts.Variables("System::PackageName").Value.ToString
        Dim bolPackageFailed As Boolean = CBool(Dts.Variables("User::ErrorHandling_PackageFailed").Value.ToString)
        Dim strPackageCompletionState As String

        strEmailTaskOutcomes = "Database counts of principals, custom roles and assignments with permissions which were backed up:" & vbCrLf & "Total Databases Processed: " & intTotalDBCount.ToString & vbCrLf &
            "Completed Databases: " & intCompletedDBCount.ToString & vbCrLf &
            "Failed Databases: " & intFailedDBCount.ToString & vbCrLf &
            "Detailed information available in the summary and detail tables"

        strEmailMessage = "SERVER: " + strServerName + vbCrLf + "DATABASE PARAMETERS: " + strDatabasesParameters + vbCrLf + "DATABASES (Process All): " + strDatabasesParameters2 + vbCrLf
        If intFailedDBCount > 0 Then
            bolPackageFailed = True
        End If
        If bolPackageFailed = False Then
            ' the package did not fail, check if failed database count > 0
            strPackageCompletionState = "SUCCESS"
            strEmailSubject = strPackageCompletionState & "--" + Dts.Variables("System::PackageName").Value.ToString + " SSIS Package"
            strEmailMessage = strEmailMessage & "PACKAGE " & strPackageCompletionState & "--All tasks in the " & Dts.Variables("System::PackageName").Value.ToString & " package completed successfully.  Detailed information is:" & vbCrLf & vbCrLf & strEmailTaskOutcomes & vbCrLf & vbCrLf & strEmailMessageFooter & vbCrLf
            strEmailCC = ""

        Else
            ' the package did fail--set email text values accordingly
            strPackageCompletionState = "FAILURE"
            strEmailSubject = strPackageCompletionState & "--" + Dts.Variables("System::PackageName").Value.ToString + " SSIS Package"
            strEmailMessage = strEmailMessage & "PACKAGE " & strPackageCompletionState & "--At least one task in the " & Dts.Variables("System::PackageName").Value.ToString & " package failed.  Detailed information is:" & vbCrLf & strEmailTaskOutcomes & vbCrLf & vbCrLf & strEmailMessageFooter & vbCrLf

            strEmailCC = "sql_alerts@kyfb.com"
            ' No need to send Email CC if the notification is going to same address
            If InStr(strEmailCC, Dts.Variables("User::ErrorHandling_Email_ToLine").Value.ToString, CompareMethod.Text) <> 0 Then
                strEmailCC = ""
            End If
            ' The next task will send an email if the variable User::ErrorHandling_SendEmailOnSuccess is set to TRUE.
            ' Since failure emails should always be sent, override the disabling of email sending
            Dts.Variables("User::ErrorHandling_SendEmailOnSuccess").Value = True

        End If
        Dts.Variables("User::ErrorHandling_Email_Subject").Value = strEmailSubject
        Dts.Variables("User::ErrorHandling_Email_Message").Value = strEmailMessage
        Dts.Variables("User::ErrorHandling_Email_CC").Value = strEmailCC
        Dts.TaskResult = ScriptResults.Success
    End Sub

End Class]]></ProjectItem>
