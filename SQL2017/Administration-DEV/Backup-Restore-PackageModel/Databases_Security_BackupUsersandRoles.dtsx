<CLSCompliant(False)>
Public Class ScriptMain
    Inherits UserComponent

    Dim strExcludedDatabaseNames As String
    Dim strExcludedDatabaseName As String

    Dim i As Integer
    Public Overrides Sub PreExecute()
        MyBase.PreExecute()
        strExcludedDatabaseNames = Trim(Me.Variables.BackupParametersDatabasesToExclude.ToString())
        i = 0
    End Sub

    Public Overrides Sub PostExecute()
        MyBase.PostExecute()

    End Sub

    Public Overrides Sub CreateNewOutputRows()
        If Len(strExcludedDatabaseNames) > 0 Then
            Dim arrDatabasesToExclude() As String = Split(strExcludedDatabaseNames, ",", , CompareMethod.Text)
            For i = 0 To arrDatabasesToExclude.Length - 1
                strExcludedDatabaseName = LTrim(RTrim(arrDatabasesToExclude(i)))
                DatabasesListBuffer.AddRow()
                DatabasesListBuffer.DatabaseName = strExcludedDatabaseName
                DatabasesListBuffer.IsNamedExclusion = True
            Next
        End If

    End Sub

End Class
