                      <ProjectItem
                        Name="ScriptMain.vb"
                        Encoding="UTF8"><![CDATA[' Microsoft SQL Server Integration Services Script Task
' Write scripts using Microsoft Visual Basic 2008.
' The ScriptMain is the entry point class of the script.

Imports System
Imports System.Data
Imports System.Math
Imports Microsoft.SqlServer.Dts.Runtime

<Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute> _
<System.CLSCompliantAttribute(False)> _
Partial Public Class ScriptMain
	Inherits Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase

	Enum ScriptResults
		Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
		Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
	End Enum
	

    Public Sub Main()
        'System::SourceName
        'RW: User::Database_Name_01,User::ErrorHandling_BackupUsersFailed_Ind,User::ErrorHandling_TaskResults,User::Server_Name_01
        Dim strSourceName As String = Dts.Variables("System::SourceName").Value.ToString
        Dim strDatabaseName As String = Dts.Variables("User::Database_Name_01").Value.ToString
        Dim strServerName As String = Dts.Variables("User::Server_Name_01").Value.ToString
        Dim strTaskResults As String = Dts.Variables("User::ErrorHandling_TaskResults").Value.ToString
        ' This on error event fires if the SQL script creating the tables holding the database cannot be created
        If CBool(Dts.Variables("User::ErrorHandling_BackupUsersFailed_Ind").Value.ToString) = False Then

            strTaskResults = strTaskResults & vbCrLf & "COMPLETED:" & strSourceName & "--database principals and memberships for " & strDatabaseName & " on server " & strServerName & " recorded to backup tables"
            Dts.Variables("User::ErrorHandling_TaskResults").Value = strTaskResults
        End If

        Dts.TaskResult = ScriptResults.Success
    End Sub

End Class]]></ProjectItem>
