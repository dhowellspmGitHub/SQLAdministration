                              dataType="System.String"><![CDATA[' Microsoft SQL Server Integration Services Script Component
' Write scripts using Microsoft Visual Basic 2008.
' ScriptMain is the entry point class of the script.

Imports System
Imports System.Data
Imports System.Math
Imports Microsoft.SqlServer.Dts.Pipeline.Wrapper
Imports Microsoft.SqlServer.Dts.Runtime.Wrapper
Imports System.DirectoryServices
<Microsoft.SqlServer.Dts.Pipeline.SSISScriptComponentEntryPointAttribute()> _
<CLSCompliant(False)> _
Public Class ScriptMain
    Inherits UserComponent
    'Dim i As Integer = 0
    'Dim j As Integer = 0
    Dim ADGrpResults As SearchResultCollection
    Dim ADGrpEntry As DirectoryEntry
    Dim ADGrpSearcher As DirectorySearcher
    Dim AdsPath As String
    Dim CustomLogText As String = String.Empty
    
    Public Overrides Sub PreExecute()
        MyBase.PreExecute()
        Try
            AdsPath = Me.Variables.ADObjectAdsPath
            ADGrpEntry = New DirectoryEntry(AdsPath)
            ADGrpSearcher = New DirectorySearcher(ADGrpEntry)
            ADGrpSearcher.Filter = "(objectCategory=group)"
            ADGrpSearcher.SearchScope = SearchScope.OneLevel
            ADGrpSearcher.ClientTimeout = New TimeSpan(0, 10, 0)
            ADGrpResults = ADGrpSearcher.FindAll()
            CustomLogText = AdsPath.ToString + "--Contains " + ADGrpResults.Count.ToString + " groups"
            Me.ComponentMetaData.FireInformation(0, Me.Variables.TaskName.ToString, CustomLogText, String.Empty, 0, False)

        Catch ex As Exception
            Dim strInnerMsg As String = "--Error processing: " + AdsPath.ToString
            Dim exInnerException As Exception
            Do While ex.InnerException IsNot Nothing
                strInnerMsg = strInnerMsg + ex.InnerException.Message.ToString
                exInnerException = ex.InnerException
            Loop
            Me.ComponentMetaData.FireWarning(-1, Me.Variables.TaskName.ToString, strInnerMsg, String.Empty, 0)
        End Try

    End Sub

    Public Overrides Sub CreateNewOutputRows()
        Try
            If ADGrpResults IsNot Nothing Then
                If ADGrpResults.Count > 0 Then
                    For Each ADSearchResult As SearchResult In ADGrpResults
                        Dim ADEntry As DirectoryEntry = ADSearchResult.GetDirectoryEntry()
                        ' Through the use of ADEntry.Properties("groupType").Value < 0, distribution groups are filtered out
                        ' see link http://blogs.technet.com/b/heyscriptingguy/archive/2004/12/21/how-can-i-tell-whether-a-group-is-a-security-group-or-a-distribution-group.aspx
                        ' however, later package steps loading AD group members would see group members that are distribution groups as orphaned records and trigger a load error.
                        ' to remove this as a possible data anomaly, both security and distribution groups will be loaded to the temp tables and to the actual tables
                        ' But the AD group members load will NOT process this group as a group member but exclude it via package routing in the write to the ADGM actual table
                        If ADEntry.Properties.Contains("groupType") = True Then 'And ADEntry.Properties("groupType").Value < 0 Then
                            With VBSADGroupsBuffer
                                .AddRow()
                                Dim EntrySID As Byte() = DirectCast(ADEntry.Properties("objectSid").Value, Byte())
                                .sid = EntrySID

                                ' GroupName column
                                If ADEntry.Properties.Contains("cn") = True Then
                                    If ADEntry.Properties("cn").Value.ToString IsNot Nothing Then
                                        .cn = ADEntry.Properties("cn").Value.ToString
                                    Else
                                        .cn = String.Empty
                                    End If
                                Else
                                    .cn = String.Empty
                                End If

                                If ADEntry.Properties.Contains("description") = True Then
                                    If ADEntry.Properties("description").Value.ToString IsNot Nothing Then
                                        .description = ADEntry.Properties("description").Value.ToString
                                    Else
                                        .description = String.Empty
                                    End If
                                Else
                                    .description = String.Empty
                                End If

                                If ADEntry.Properties.Contains("distinguishedName") = True Then
                                    If ADEntry.Properties("distinguishedName").Value.ToString IsNot Nothing Then
                                        .distinguishedname = ADEntry.Properties("distinguishedName").Value.ToString
                                    Else
                                        .distinguishedname = String.Empty
                                    End If
                                Else
                                    .distinguishedname = String.Empty
                                End If

                                If ADEntry.Properties.Contains("whenCreated") = True Then
                                    If ADEntry.Properties("whenCreated").Value.ToString IsNot Nothing Then
                                        .whenCreated = ADEntry.Properties("whenCreated").Value
                                    Else
                                        .whenCreated = CDate("01/01/1900")
                                    End If
                                Else
                                    .whenCreated = CDate("01/01/1900")
                                End If


                                If ADEntry.Properties.Contains("whenChanged") = True Then
                                    If ADEntry.Properties("whenChanged").Value.ToString IsNot Nothing Then
                                        .whenChanged = ADEntry.Properties("whenChanged").Value
                                    Else
                                        .whenChanged = CDate("01/01/1900")
                                    End If
                                Else
                                    .whenChanged = CDate("01/01/1900")
                                End If

                                If ADEntry.Properties.Contains("displayName") = True Then
                                    If ADEntry.Properties("displayname").Value.ToString IsNot Nothing Then
                                        .displayname = ADEntry.Properties("displayname").Value.ToString
                                    Else
                                        .displayname = String.Empty
                                    End If
                                Else
                                    .displayname = String.Empty
                                End If

                                ' Name property
                                If ADEntry.Name.ToString IsNot Nothing Then
                                    .name = ADEntry.Name.ToString
                                Else
                                    .name = String.Empty
                                End If

                                ' GUID property
                                If ADEntry.Guid.ToString IsNot Nothing Then
                                    .guid = ADEntry.Guid
                                Else
                                    .guid = Guid.Empty
                                End If

                                ' sAMAccountName property
                                If ADEntry.Properties.Contains("sAMAccountName") = True Then
                                    If ADEntry.Properties("sAMAccountName").Value.ToString IsNot Nothing Then
                                        .samaccountname = ADEntry.Properties("sAMAccountName").Value.ToString
                                    Else
                                        .samaccountname = String.Empty
                                    End If
                                Else
                                    .samaccountname = String.Empty
                                End If

                                ' sAMAccountType
                                If ADEntry.Properties.Contains("samaccounttype") = True Then
                                    If ADEntry.Properties("samaccounttype").Value.ToString IsNot Nothing Then
                                        .samaccounttype = CInt(ADEntry.Properties("samaccounttype").Value.ToString)
                                    Else
                                        .samaccounttype = 1
                                    End If
                                Else
                                    .samaccounttype = 1
                                End If

                                ' groupType column

                                If ADEntry.Properties.Contains("groupType") = True Then
                                    If ADEntry.Properties("groupType").Value.ToString IsNot Nothing Then
                                        .grouptype = CInt(ADEntry.Properties("groupType").Value.ToString) ' Is security group -- based upon preliminary IF statement
                                        ' set issecuritygroup flag based upon integer value of groupType property
                                        If CInt(ADEntry.Properties("groupType").Value.ToString) < 0 Then
                                            .issecuritygroup = True
                                        Else
                                            .issecuritygroup = False
                                        End If
                                    Else
                                        .issecuritygroup = False
                                        .grouptype = 1
                                    End If
                                Else
                                    .issecuritygroup = False
                                    .grouptype = 1
                                End If

                                If ADEntry.Properties.Contains("objectCategory") = True Then
                                    If ADEntry.Properties("objectCategory").Value.ToString IsNot Nothing Then
                                        .objectCategory = ADEntry.Properties("objectCategory").Value.ToString
                                    Else
                                        .objectCategory = String.Empty
                                    End If
                                Else
                                    .objectCategory = String.Empty
                                End If


                                ' Name property
                                If ADEntry.SchemaClassName.ToString IsNot Nothing Then
                                    .schemaclassname = ADEntry.SchemaClassName.ToString
                                Else
                                    .schemaclassname = String.Empty
                                End If

                                If ADEntry.SchemaEntry.ToString IsNot Nothing Then
                                    .schemaentry = ADEntry.SchemaEntry.ToString
                                Else
                                    .schemaentry = String.Empty
                                End If

                                If ADEntry.Properties.Contains("department") = True Then
                                    If ADEntry.Properties("department").Value.ToString IsNot Nothing Then
                                        .department = ADEntry.Properties("department").Value.ToString
                                    Else
                                        .department = String.Empty
                                    End If
                                Else
                                    .department = String.Empty
                                End If

                                If ADEntry.Properties.Contains("managedBy") = True Then
                                    If ADEntry.Properties("managedBy").Value.ToString IsNot Nothing Then
                                        .managedby = ADEntry.Properties("managedBy").Value.ToString
                                    Else
                                        .managedby = String.Empty
                                    End If
                                Else
                                    .managedby = String.Empty
                                End If
                            End With
                        End If
                        ADGrpEntry.Dispose()
                    Next
                    'Me.ComponentMetaData.FireInformation(0, "Load AD Groups", "Total Group Count=" + j.ToString + vbTab + "Security Groups Loaded to table=" + i.ToString, String.Empty, 0, False)
                Else
                    CustomLogText = AdsPath.ToString + "--Contains no groups"
                    Me.ComponentMetaData.FireInformation(0, Me.Variables.TaskName.ToString, CustomLogText, String.Empty, 0, False)

                End If
                ADGrpResults.Dispose()
            End If

        Catch ex As Exception
            Dim strInnerMsg As String = ex.Source.ToString
            Dim exInnerException As Exception
            Do While ex.InnerException IsNot Nothing
                strInnerMsg = strInnerMsg + ex.InnerException.Message.ToString
                exInnerException = ex.InnerException
            Loop
            Me.ComponentMetaData.FireError(-1, Me.Variables.TaskName.ToString, strInnerMsg, String.Empty, 0, True)

        End Try
    End Sub
    Public Overrides Sub PostExecute()
        MyBase.PostExecute()
        ADGrpSearcher.Dispose()
        

    End Sub
End Class
]]></arrayElement>
