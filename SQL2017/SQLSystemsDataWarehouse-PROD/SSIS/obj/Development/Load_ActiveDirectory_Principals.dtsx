Public Class ScriptMain
    Inherits UserComponent
    Dim ADUsersResults As SearchResultCollection
    Dim ADUsersEntry As DirectoryEntry
    Dim ADUsersSearcher As DirectorySearcher
    Dim CustomLogText As String = String.Empty
    Dim AdsPath As String
    Dim i As Integer = 0
    Dim j As Integer = 0
    Dim x As String()
    Dim y As String

    Public Overrides Sub PreExecute()
        MyBase.PreExecute()
        Try
            AdsPath = Me.Variables.ADObjectAdsPath
            'ADUsersEntry = New DirectoryEntry(AdsPath, "KFBDOM1\DXHADMIN", "PEN1865cil")
            ADUsersEntry = New DirectoryEntry(AdsPath)
            ADUsersSearcher = New DirectorySearcher(ADUsersEntry)
            ADUsersSearcher.Filter = "(&(objectCategory=person)(objectClass=user))"
            'ADUsersSearcher.Filter = "*(&(objectClass=user))"
            ADUsersSearcher.SearchScope = SearchScope.OneLevel
            ADUsersSearcher.PageSize = 2000
            ADUsersSearcher.ClientTimeout = New TimeSpan(0, 5, 0)
            ADUsersResults = ADUsersSearcher.FindAll()
        Catch ex As Exception
            Dim strInnerMsg As String = "Exception firing at PreExecute for--" + AdsPath.ToString
            Dim exInnerException As Exception
            Do While ex.InnerException IsNot Nothing
                strInnerMsg = strInnerMsg + ex.InnerException.Message.ToString
                exInnerException = ex.InnerException
            Loop
            Me.ComponentMetaData.FireWarning(0, Me.Variables.TaskName.ToString, strInnerMsg.ToString, String.Empty, 0)
        End Try

    End Sub
    Public Overrides Sub CreateNewOutputRows()
        Try
            If ADUsersResults IsNot Nothing Then
                j = ADUsersResults.Count
                If ADUsersResults.Count > 0 Then
                    For Each ADSearchResult As SearchResult In ADUsersResults
                        Dim ADUser As DirectoryEntry
                        ADUser = ADSearchResult.GetDirectoryEntry()
                        With VBSADUsersBuffer
                            .AddRow()
                            Dim EntrySID As Byte() = DirectCast(ADUser.Properties("objectSid").Value, Byte())
                            .sid = EntrySID

                            If ADUser.Properties.Contains("cn") = True Then
                                If ADUser.Properties("cn").Value IsNot Nothing Then
                                    .cn = ADUser.Properties("cn").Value.ToString
                                Else
                                    .cn = String.Empty
                                End If
                            End If

                            If ADUser.Properties.Contains("objectcategory") = True Then
                                If ADUser.Properties("objectcategory").Value IsNot Nothing Then
                                    .objectcategory = ADUser.Properties("objectcategory").Value.ToString
                                Else
                                    .objectcategory = String.Empty
                                End If
                            End If

                            If ADUser.Properties.Contains("userprincipalname") = True Then
                                If ADUser.Properties("userprincipalname").Value IsNot Nothing Then
                                    .userprincipalname = ADUser.Properties("userprincipalname").Value.ToString
                                Else
                                    .userprincipalname = String.Empty
                                End If
                            End If

                            If ADUser.Properties.Contains("samaccounttype") = True Then
                                If ADUser.Properties("samaccounttype").Value IsNot Nothing Then
                                    .samaccounttype = ADUser.Properties("samaccounttype").Value
                                Else
                                    .samaccounttype = 0
                                End If
                            End If


                            If ADUser.Properties.Contains("primarygroup") = True Then
                                If ADUser.Properties("primarygroup").Value IsNot Nothing Then
                                    .primarygroup = ADUser.Properties("primarygroup").Value
                                Else
                                    .primarygroup = 0
                                End If
                            End If


                            If ADUser.Properties.Contains("description") = True Then
                                If ADUser.Properties("description").Value IsNot Nothing Then
                                    .description = ADUser.Properties("description").Value.ToString
                                Else
                                    .description = String.Empty
                                End If
                            End If

                            If ADUser.Properties.Contains("displayname") = True Then
                                If ADUser.Properties("displayname").Value IsNot Nothing Then
                                    .displayname = ADUser.Properties("displayname").Value.ToString
                                Else
                                    .displayname = String.Empty
                                End If
                            End If


                            If ADUser.Properties.Contains("whenchanged") = True Then
                                If ADUser.Properties("whenchanged").Value IsNot Nothing Then
                                    .whenchanged = ADUser.Properties("whenchanged").Value.ToString
                                Else
                                    .whenchanged = CDate("01/01/1900")
                                End If
                            End If

                            If ADUser.Properties.Contains("whencreated") = True Then
                                If ADUser.Properties("whencreated").Value IsNot Nothing Then
                                    .whencreated = ADUser.Properties("whencreated").Value.ToString
                                Else
                                    .whencreated = CDate("01/01/1900")
                                End If
                            End If

                            If ADUser.Properties.Contains("distinguishedname") = True Then
                                If ADUser.Properties("distinguishedname").Value IsNot Nothing Then
                                    .distinguishedname = ADUser.Properties("distinguishedname").Value.ToString
                                Else
                                    .distinguishedname = String.Empty
                                End If
                            End If


                            If ADUser.Name.ToString IsNot Nothing Then
                                .name = ADUser.Name.ToString
                            Else
                                .name = String.Empty
                            End If

                            If ADUser.Guid.ToString IsNot Nothing Then
                                .guid = ADUser.Guid
                            Else
                                .guid = Guid.Empty
                            End If

                            If ADUser.Properties.Contains("samaccountname") = True Then
                                If ADUser.Properties("samaccountname").Value IsNot Nothing Then
                                    .samaccountname = ADUser.Properties("samaccountname").Value.ToString
                                Else
                                    .samaccountname = String.Empty
                                End If
                            End If

                            If ADUser.Properties.Contains("givenname") = True Then
                                If ADUser.Properties("givenname").Value IsNot Nothing Then
                                    .givenname = ADUser.Properties("givenname").Value.ToString
                                Else
                                    .givenname = String.Empty
                                End If
                            End If

                            If ADUser.Properties.Contains("sn") = True Then
