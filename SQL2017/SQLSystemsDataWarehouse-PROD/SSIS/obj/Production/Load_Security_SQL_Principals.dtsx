Imports Microsoft.SqlServer.Dts.Runtime
#End Region

'ScriptMain is the entry point class of the script.  Do not change the name, attributes,
'or parent of this class.
<Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute()> _
<System.CLSCompliantAttribute(False)> _
Partial Public Class ScriptMain
    Inherits Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
    Public Sub Main()
		'RO: User::ErrorHandling_ErrorCount,User::ErrorHandling_WarningCount,System::ExecutionInstanceGUID,System::PackageName,$Project::Database_Name_01
		'RW: User::Email_Source,User::Email_Subject,User::Email_ToLine,User::ErrorHandling_SendEmail,User::InitiatingJobName,User::InitiatingJobScheduler

		Dim strPackageName As String = Dts.Variables("System::PackageName").Value.ToString
		Dim strInitiatingJobName As String = Dts.Variables("User::InitiatingJobName").Value.ToString
		Dim strInitiatingJobScheduler As String = Dts.Variables("User::InitiatingJobScheduler").Value.ToString
		Dim strExecutionID As String = Dts.Variables("System::ExecutionInstanceGUID").Value.ToString
		Dim strEmailFooter As String = "Package Name: " + strPackageName + vbCrLf + "Job Name: " + strInitiatingJobName + vbCrLf + "Execution: " + strExecutionID
		Dim strDatabaseName01 As String = Dts.Variables("$Project::Database_Name_01").Value.ToString
		Dim intErrorCount As Integer = CInt(Dts.Variables("User::ErrorHandling_ErrorCount").Value.ToString)
		Dim intWarningCount As Integer = CInt(Dts.Variables("User::ErrorHandling_WarningCount").Value.ToString)


		Dim bolSendEmail As Boolean = False
		Dim strEmailSource As String = String.Empty
		Dim strDatabasesErrorText As String = String.Empty
		Dim strDatabasesWarningText As String = String.Empty
		Dim strPackageCompletionState As String = String.Empty

		'If others are notified, include SQL Admins in notice of failure.  If no one is notified, at least notify SQL admins
		If Len(Dts.Variables("User::Email_ToLine").Value.ToString) = 0 Then
			Dts.Variables("User::Email_ToLine").Value = "sql_alerts@kyfb.com"
		End If

		If intWarningCount > 0 Then
			bolSendEmail = True
			strDatabasesWarningText = "SQL Server and Database Principals:" + vbCrLf +
				"Count of Tables having non-fatal errors (warnings):" + intWarningCount.ToString() + vbCrLf +
				"Errors indicate:" + vbCrLf +
				"At least one Server/Database Principal, Role or RoleMember record could not be loaded to the staging tables" + vbCrLf +
				"Please review 'staging.ErrorHandling... tables for further investigation" + vbCrLf
			strPackageCompletionState = "WARNINGS"

		End If

		If intErrorCount > 0 Then
			bolSendEmail = True
			strDatabasesErrorText = "Active Directory Users and Groups:" + vbCrLf +
				"Count of Tables having errors:" + intErrorCount.ToString() + vbCrLf +
				"Errors indicate:" + vbCrLf +
				"At least one AServer/Database Principal, Role or RoleMember record could not be loaded to the security tables" + vbCrLf +
				"Please review 'security.ErrorHandling... tables for further investigation" + vbCrLf
			strPackageCompletionState = "ERRORS"
		End If

		Dts.Variables("User::Email_Subject").Value = strPackageCompletionState + "--" + strPackageName
		strEmailSource = "The package returned: " + LCase(strPackageCompletionState) + vbCrLf + strDatabasesErrorText + vbCrLf + strDatabasesWarningText + vbCrLf +
		  "Summary and detail logging tables are in the SSIS Execution Catalog viewable using the Integration Service Catalogs--Reports" + vbCrLf + strEmailFooter

		Dts.Variables("User::Email_Source").Value = Left(Trim(strEmailSource), 4000)
		Dts.Variables("User::ErrorHandling_SendEmail").Value = bolSendEmail

		Dts.TaskResult = ScriptResults.Success
	End Sub

#Region "ScriptResults declaration"
    'This enum provides a convenient shorthand within the scope of this class for setting the
    'result of the script.

    'This code was generated automatically.
    Enum ScriptResults
        Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
        Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
    End Enum

#End Region

End Class]]></ProjectItem>
