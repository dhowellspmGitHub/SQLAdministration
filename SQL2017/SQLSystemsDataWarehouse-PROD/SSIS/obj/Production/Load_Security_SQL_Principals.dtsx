          SQLTask:SqlStatementSource="--TRUNCATE ErrorHandling tables&#xA;TRUNCATE TABLE [staging].[ErrorHandling_SQLPrincipals]&#xA;TRUNCATE TABLE [staging].[ErrorHandling_DatabasesPrincipals]&#xA;TRUNCATE TABLE [staging].[ErrorHandling_ServerPrincipals]&#xA;TRUNCATE TABLE [staging].[ErrorHandling_ServerRoleMembers]&#xA;TRUNCATE TABLE [staging].[ErrorHandling_ServerRoles]&#xA;TRUNCATE TABLE [security].[ErrorHandling_ServerPrincipals]&#xA;TRUNCATE TABLE [security].[ErrorHandling_ServerRoleMembers]&#xA;TRUNCATE TABLE [security].[ErrorHandling_DatabasePrincipals]&#xA;TRUNCATE TABLE [security].[ErrorHandling_DatabaseRoleMembers]&#xA;&#xA;IF NOT EXISTS (SELECT * FROM sys.objects WHERE name like 'tmpServerRoles' and OBJECT_SCHEMA_NAME(object_id) like 'staging' and type like 'U')&#xA;BEGIN&#xA;CREATE TABLE [staging].[tmpServerRoles](&#xA;&#x9;[RolePrincipalId] [int] NULL,&#xA;&#x9;[ServerInstanceID] [int] NOT NULL,&#xA;&#x9;[SID] [varbinary](85) NULL,&#xA;&#x9;[Name] [varchar](128) NULL,&#xA;&#x9;[create_date] [datetime] NULL,&#xA;&#x9;[modify_date] [datetime] NULL&#xA;) ON [DATAFG1]&#xA;END&#xA;ELSE&#xA;BEGIN&#xA;TRUNCATE TABLE [staging].[tmpServerRoles]&#xA;END &#xA;&#xA;&#xA;IF NOT EXISTS (SELECT * FROM sys.objects WHERE name like 'tmpServerPrincipals' and OBJECT_SCHEMA_NAME(object_id) like 'staging' and type like 'U')&#xA;BEGIN&#xA;CREATE TABLE [staging].[tmpServerPrincipals](&#xA;&#x9;[ServerInstanceID] [int] NOT NULL,&#xA;&#x9;[PrincipalID] [int] NULL,&#xA;&#x9;[Name] [varchar](128) NOT NULL,&#xA;&#x9;[SID] [varbinary](85) NULL,&#xA;&#x9;[type] [varchar](1) NOT NULL,&#xA;&#x9;[type_desc] [nvarchar] (60) NULL,&#xA;&#x9;[IsServerRoleInd] [bit] NOT NULL,&#xA;&#x9;[IsDisabledInd] [bit] NULL,&#xA;&#x9;[ADAccountInd] [bit] NULL,&#xA;&#x9;[ADGroupInd] [bit] NULL,&#xA;&#x9;[CreateDt] [datetime] NULL,&#xA;&#x9;[ModifiedDt] [datetime] NULL&#xA;) ON [DATAFG1]&#xA;END&#xA;ELSE&#xA;BEGIN&#xA;TRUNCATE TABLE [staging].[tmpServerPrincipals]&#xA;END &#xA;&#xA;IF NOT EXISTS (SELECT * FROM sys.objects WHERE name like 'tmpServerRoleMembers' and OBJECT_SCHEMA_NAME(object_id) like 'staging' and type like 'U')&#xA;BEGIN&#xA;CREATE TABLE [staging].[tmpServerRoleMembers](&#xA;&#x9;[ServerInstanceID] [int] NOT NULL,&#xA;&#x9;[RolePrincipalID] [int] NOT NULL,&#xA;&#x9;[MemberPrincipalID] [int] NOT NULL,&#xA;&#x9;[IsServerRoleInd] [bit] NOT NULL&#xA;) ON [DATAFG1]&#xA;END&#xA;ELSE&#xA;BEGIN&#xA;TRUNCATE TABLE [staging].[tmpServerRoleMembers]&#xA;END &#xA;&#xA;&#xA;IF NOT EXISTS (SELECT * FROM sys.objects WHERE name like 'tmpDatabaseRoles' and OBJECT_SCHEMA_NAME(object_id) like 'staging' and type like 'U')&#xA;BEGIN&#xA;CREATE TABLE [staging].[tmpDatabaseRoles](&#xA;&#x9;[DatabaseUniqueID] [int] NOT NULL,&#xA;&#x9;[principal_id] [int] NOT NULL,&#xA;&#x9;[Name] [varchar](128) NULL,&#xA;&#x9;[type] [varchar](1) NULL,&#xA;&#x9;[sid] [varbinary](85) NULL,&#xA;&#x9;[create_date] [datetime] NULL,&#xA;&#x9;[modify_date] [datetime] NULL&#xA;) ON [DATAFG1]&#xA;END&#xA;ELSE&#xA;BEGIN&#xA;TRUNCATE TABLE [staging].[tmpDatabaseRoles]&#xA;END &#xA;&#xA;&#xA;IF NOT EXISTS (SELECT * FROM sys.objects WHERE name like 'tmpDatabasePrincipals' and OBJECT_SCHEMA_NAME(object_id) like 'staging' and type like 'U')&#xA;BEGIN&#xA;CREATE TABLE [staging].[tmpDatabasePrincipals](&#xA;&#x9;[DatabaseUniqueID] [int] NOT NULL,&#xA;&#x9;[PrincipalID] [int] NULL,&#xA;&#x9;[UserName] [varchar](128) NULL,&#xA;&#x9;[sid] [varbinary](85) NULL,&#xA;&#x9;[type] [char](1) NOT NULL,&#xA;&#x9;[type_desc] [nvarchar] (60) NULL,&#xA;&#x9;[IsDatabaseRoleInd] [bit] NOT NULL,&#xA;&#x9;[CreateDt] [datetime] NOT NULL,&#xA;&#x9;[ModifyDt] [datetime] NULL&#xA;) ON [DATAFG1]&#xA;END&#xA;ELSE&#xA;BEGIN&#xA;TRUNCATE TABLE [staging].[tmpDatabasePrincipals]&#xA;END &#xA;&#xA;IF NOT EXISTS (SELECT * FROM sys.objects WHERE name like 'tmpDatabaseRoleMembers' and OBJECT_SCHEMA_NAME(object_id) like 'staging' and type like 'U')&#xA;BEGIN&#xA;CREATE TABLE [staging].[tmpDatabaseRoleMembers](&#xA;&#x9;[DatabaseUniqueID] [int] NOT NULL,&#xA;&#x9;[RolePrincipalId] [int] NULL,&#xA;&#x9;[MemberPrincipalID] [int] NULL,&#xA;&#x9;[IsDatabaseRoleInd] [bit] NOT NULL&#xA;) ON [DATAFG1]&#xA;END&#xA;ELSE&#xA;BEGIN&#xA;TRUNCATE TABLE [staging].[tmpDatabaseRoleMembers]&#xA;END" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
