    <DTS:Executable
      DTS:refId="Package\SQL Test for ETL Process restart"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{50DA3EC3-0E75-4E57-AF08-3C914A19729C}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="SQL Test for ETL Process restart"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2012; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:LoggingOptions
        DTS:FilterKind="0"
        DTS:LoggingMode="2" />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{7B8D702A-E4B9-4BA5-8B40-1DC55EDF6964}"
          SQLTask:SqlStatementSource="/****** &#xA;Query gets the most recent ETL process ID for the current date if it exists.  This step is needed to re-use the ETL process ID for completion of the package on restart using the the same ETL process ID for the given date&#xA;******/&#xA;DECLARE @ETLProcess_ID INT&#xA;DECLARE @ETLProcess_GroupID int&#xA;DECLARE @InitiatingJobName nvarchar(128)&#xA;DECLARE @ETLProcess_Date date&#xA;DECLARE @CurrentRecordsInd bit&#xA;DECLARE @ProcessFailedInd bit&#xA;&#xA;SELECT @ETLProcess_ID = [ETLProcessID]&#xA;,@ETLProcess_GroupID = [ETLProcessGroupID]&#xA;,@InitiatingJobName =  [InitiatingJobName]&#xA;,@ETLProcess_Date =  [ProcessDt]&#xA;,@CurrentRecordsInd = [CurrentRecordsInd]&#xA;,@ProcessFailedInd = [ProcessFailedInd]&#xA;FROM [dbo].[ETLProcesses]&#xA;WHERE [CurrentRecordsInd] = 1&#xA;AND [ProcessDt] = CAST(@P1 AS DATE)&#xA;AND [ETLProcessGroupID] = @P2&#xA;&#xA;&#xA;SET @ETLProcess_Date= ISNULL(@ETLProcess_Date,CAST(DATEADD(d,-1,@P1) AS DATE))&#xA;SELECT &#xA;ISNULL(@ETLProcess_ID,0) AS ETLProcess_ID&#xA;,ISNULL(@ETLProcess_GroupID,@P2) AS ETLProcess_GroupID&#xA;,ISNULL(@InitiatingJobName, '') AS InitiatingJobName&#xA;,@ETLProcess_Date AS ETLProcess_Date&#xA;,ISNULL(@CurrentRecordsInd,0) AS CurrentRecordsInd&#xA;,ISNULL(@ProcessFailedInd,0) AS ProcessFailedInd&#xA;,CASE WHEN @ETLProcess_Date  = CAST(GETDATE() AS DATE) THEN 1 ELSE 0 END AS ETLProcess_RestartInd"
          SQLTask:ResultType="ResultSetType_SingleRow" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
          <SQLTask:ResultBinding
            SQLTask:ResultName="0"
            SQLTask:DtsVariableName="User::ETLProcess_ID" />
          <SQLTask:ResultBinding
            SQLTask:ResultName="3"
            SQLTask:DtsVariableName="User::ETLProcess_Date" />
          <SQLTask:ResultBinding
            SQLTask:ResultName="6"
            SQLTask:DtsVariableName="User::ETLProcess_RestartInd" />
          <SQLTask:ParameterBinding
            SQLTask:ParameterName="@P1"
            SQLTask:DtsVariableName="System::StartTime"
            SQLTask:ParameterDirection="Input"
            SQLTask:DataType="6"
            SQLTask:ParameterSize="-1" />
          <SQLTask:ParameterBinding
            SQLTask:ParameterName="@P2"
            SQLTask:DtsVariableName="User::ETLProcess_GroupID"
            SQLTask:ParameterDirection="Input"
            SQLTask:DataType="11"
            SQLTask:ParameterSize="-1" />
          <SQLTask:ParameterBinding
            SQLTask:ParameterName="@P3"
            SQLTask:DtsVariableName="User::InitiatingJobName"
            SQLTask:ParameterDirection="Input"
            SQLTask:DataType="16"
            SQLTask:ParameterSize="-1" />
        </SQLTask:SqlTaskData>
