          SQLTask:SqlStatementSource="--TRUNCATE ErrorHandling tables&#xA;TRUNCATE TABLE [staging].[ErrorHandling_SQLPrincipals]&#xA;TRUNCATE TABLE [staging].[ErrorHandling_DatabasesPrincipals]&#xA;TRUNCATE TABLE [staging].[ErrorHandling_ServerPrincipals]&#xA;TRUNCATE TABLE [staging].[ErrorHandling_ServerRoleMembers]&#xA;TRUNCATE TABLE [staging].[ErrorHandling_ServerRoles]&#xA;&#xA;&#xA;&#xA;IF NOT EXISTS (SELECT * FROM sys.objects WHERE name like 'tmpServerRoles' and OBJECT_SCHEMA_NAME(object_id) like 'staging' and type like 'U')&#xA;BEGIN&#xA;CREATE TABLE [staging].[tmpServerRoles](&#xA;&#x9;[ServerInstanceID] [int] NOT NULL,&#xA;&#x9;[Name] [varchar](128) NULL,&#xA;&#x9;[RolePrincipalId] [int] NULL,&#xA;&#x9;[IsSysAdmin] [bit] NULL,&#xA;&#x9;[SID] [varbinary](85) NULL,&#xA;&#x9;[create_date] [datetime] NULL,&#xA;&#x9;[modify_date] [datetime] NULL,&#xA;&#x9;[AccessToAllDatabases] [bit] NULL,&#xA;&#x9;[CustomRoleInd] [bit] NULL&#xA;) ON [DATAFG1]&#xA;END&#xA;ELSE&#xA;BEGIN&#xA;TRUNCATE TABLE [staging].[tmpServerRoles]&#xA;END &#xA;&#xA;&#xA;IF NOT EXISTS (SELECT * FROM sys.objects WHERE name like 'tmpServerPrincipals' and OBJECT_SCHEMA_NAME(object_id) like 'staging' and type like 'U')&#xA;BEGIN&#xA;CREATE TABLE [staging].[tmpServerPrincipals](&#xA;&#x9;[ServerInstanceID] [int] NOT NULL,&#xA;&#x9;[principal_id] [int] NULL,&#xA;&#x9;[Name] [varchar](128) NOT NULL,&#xA;&#x9;[SID] [varbinary](85) NULL,&#xA;&#x9;[CreateDt] [datetime] NULL,&#xA;&#x9;[ModifiedDt] [datetime] NULL,&#xA;&#x9;[ServerPrincipalTypeCd] [varchar](1) NULL,&#xA;&#x9;[IsDisabledInd] [bit] NULL,&#xA;&#x9;[ADAccountInd] [bit] NULL,&#xA;&#x9;[ADGroupInd] [bit] NULL&#xA;) ON [DATAFG1]&#xA;END&#xA;ELSE&#xA;BEGIN&#xA;TRUNCATE TABLE [staging].[tmpServerPrincipals]&#xA;END &#xA;&#xA;IF NOT EXISTS (SELECT * FROM sys.objects WHERE name like 'tmpServerRoleMembers' and OBJECT_SCHEMA_NAME(object_id) like 'staging' and type like 'U')&#xA;BEGIN&#xA;CREATE TABLE [staging].[tmpServerRoleMembers](&#xA;&#x9;[ServerInstanceID] [int] NOT NULL,&#x9;&#xA;&#x9;[RolePrincipalID] [int] NOT NULL,&#xA;&#x9;[Principal_ID] [int] NOT NULL&#xA;) ON [DATAFG1]&#xA;END&#xA;ELSE&#xA;BEGIN&#xA;TRUNCATE TABLE [staging].[tmpServerRoleMembers]&#xA;END &#xA;&#xA;&#xA;IF NOT EXISTS (SELECT * FROM sys.objects WHERE name like 'tmpDatabaseRoles' and OBJECT_SCHEMA_NAME(object_id) like 'staging' and type like 'U')&#xA;BEGIN&#xA;CREATE TABLE [staging].[tmpDatabaseRoles](&#xA;&#x9;[DatabaseUniqueId] [int] NOT NULL,&#xA;&#x9;[principal_id] [int] NOT NULL,&#xA;&#x9;[Name] [varchar](128) NULL,&#xA;&#x9;[TYPE] [varchar](1) NULL,&#xA;&#x9;[sid] [varbinary](85) NULL,&#xA;&#x9;[CREATE_DATE] [datetime] NULL,&#xA;&#x9;[MODIFY_DATE] [datetime] NULL,&#xA;&#x9;[CustomRoleInd] [bit] NULL&#xA;) ON [DATAFG1]&#xA;END&#xA;ELSE&#xA;BEGIN&#xA;TRUNCATE TABLE [staging].[tmpDatabaseRoles]&#xA;END &#xA;&#xA;&#xA;IF NOT EXISTS (SELECT * FROM sys.objects WHERE name like 'tmpDatabasePrincipals' and OBJECT_SCHEMA_NAME(object_id) like 'staging' and type like 'U')&#xA;BEGIN&#xA;CREATE TABLE [staging].[tmpDatabasePrincipals](&#xA;&#x9;[DatabaseUniqueId] [int] NOT NULL,&#xA;&#x9;[principal_id] [int] NULL,&#xA;&#x9;[sid] [varbinary](85) NULL,&#xA;&#x9;[CREATE_DATE] [datetime] NULL,&#xA;&#x9;[MODIFY_DATE] [datetime] NULL,&#xA;&#x9;[UserName] [varchar](128) NULL&#xA;) ON [DATAFG1]&#xA;END&#xA;ELSE&#xA;BEGIN&#xA;TRUNCATE TABLE [staging].[tmpDatabasePrincipals]&#xA;END &#xA;&#xA;IF NOT EXISTS (SELECT * FROM sys.objects WHERE name like 'tmpDatabaseRoleMembers' and OBJECT_SCHEMA_NAME(object_id) like 'staging' and type like 'U')&#xA;BEGIN&#xA;CREATE TABLE [staging].[tmpDatabaseRoleMembers](&#xA;&#x9;[DatabaseUniqueId] [int] NOT NULL,&#xA;&#x9;[RolePrincipalId] [int] NULL,&#xA;&#x9;[PrincipalID] [int] NULL&#xA;) ON [DATAFG1]&#xA;END&#xA;ELSE&#xA;BEGIN&#xA;TRUNCATE TABLE [staging].[tmpDatabaseRoleMembers]&#xA;END &#xA;&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
