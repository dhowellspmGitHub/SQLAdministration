'ScriptMain is the entry point class of the script.  Do not change the name, attributes,
'or parent of this class.
<Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute()> _
<System.CLSCompliantAttribute(False)> _
Partial Public Class ScriptMain
    Inherits Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
    Public Sub Main()
		'RO: User::ErrorHandling_ErrorCount,System::ExecutionInstanceGUID,System::PackageName,$Project::Database_Name_01$Project::Database_Name_01
		'RW: User::Email_Source,User::Email_Subject,User::Email_ToLine,User::ErrorHandling_SendEmail,User::InitiatingJobName
		Dim strPackageName As String = Dts.Variables("System::PackageName").Value.ToString
        Dim strInitiatingJobName As String = Dts.Variables("$Package::InitiatingJobName").Value.ToString
        Dim strExecutionID As String = Dts.Variables("System::ExecutionInstanceGUID").Value.ToString
        Dim strEmailFooter As String = "Package Name: " + strPackageName + vbCrLf + "Job Name: " + strInitiatingJobName + vbCrLf + "Execution: " + strExecutionID
        Dim strDatabaseName01 As String = Dts.Variables("$Project::Database_Name_01").Value.ToString
		Dim intErrorCount As Integer = CInt(Dts.Variables("User::ErrorHandling_ErrorCount").Value.ToString)

		Dim bolSendEmail As Boolean = False
		Dim strEmailSource As String = String.Empty
		Dim strDatabasesErrorText As String = String.Empty
		Dim strPackageCompletionState As String = "ERRORS"

		'If others are notified, include SQL Admins in notice of failure.  If no one is notified, at least notify SQL admins
		If Len(Dts.Variables("User::Email_ToLine").Value.ToString) = 0 Then
			Dts.Variables("User::Email_ToLine").Value = "sql_alerts@kyfb.com"
		End If

		If intErrorCount > 0 Then
			bolSendEmail = True
			strDatabasesErrorText = "Active Directory Users and Groups:" + vbCrLf +
				"Count of Tables having errors:" + intErrorCount.ToString() + vbCrLf +
				"Errors indicate:" + vbCrLf +
				"At least one Active Directory User, Group or Group Member record could not be loaded" + vbCrLf +
				"Please review 'security.ErrorHandling... tables for further investigation" + vbCrLf
		End If

		Dts.Variables("User::Email_Subject").Value = strPackageCompletionState + "--" + strPackageName
		strEmailSource = "The package returned: " + LCase(strPackageCompletionState) + vbCrLf + strDatabasesErrorText +
		  "Summary and detail logging tables are in the SSIS Execution Catalog viewable using the Integration Service Catalogs--Reports" + vbCrLf + strEmailFooter

		Dts.Variables("User::Email_Source").Value = Left(Trim(strEmailSource), 4000)
		Dts.Variables("User::ErrorHandling_SendEmail").Value = bolSendEmail
		Dts.TaskResult = ScriptResults.Success
	End Sub

