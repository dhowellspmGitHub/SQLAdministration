<CLSCompliant(False)> _
Public Class ScriptMain
    Inherits UserComponent
    Dim ADDC As String
    Dim ADEntry As DirectoryEntry
    Dim ADSearcher As DirectorySearcher
    Dim ADResults As SearchResultCollection


    Public Overrides Sub PreExecute()
        MyBase.PreExecute()
        ADDC = Me.Variables.ADObjectDCName
        ADEntry = New DirectoryEntry(ADDC)
        ADSearcher = New DirectorySearcher(ADEntry)
        ADSearcher.SearchScope = SearchScope.Subtree
        ADSearcher.Filter = "(objectClass=organizationalUnit)"
    End Sub

    Public Overrides Sub PostExecute()
        MyBase.PostExecute()
        
    End Sub

    Public Overrides Sub CreateNewOutputRows()
        '' Use an enumerator object to move through the collection
       
        ADResults = ADSearcher.FindAll()
        For Each ADSearchResult As SearchResult In ADResults
            Dim ADEntry As DirectoryEntry = ADSearchResult.GetDirectoryEntry()
            With ADOUsBuffer
                .AddRow()
                .objectclass = "organizationalUnit"

                If ADEntry.Path.ToString IsNot Nothing Then
                    .adspath = ADEntry.Path.ToString
                Else
                    .adspath = "ADSPath-Unknown"
                End If

                If ADEntry.Guid.ToString IsNot Nothing Then
                    .objectguid = ADEntry.Guid
                Else
                    .objectguid = Guid.Empty
                End If

                If ADEntry.Properties("objectcategory").Value IsNot Nothing Then
                    .objectcategory = ADEntry.Properties("objectcategory").Value.ToString
                Else
                    .objectcategory = "objectCategory-Unknown"
                End If


                If ADEntry.Properties("name").Value IsNot Nothing Then
                    .name = ADEntry.Properties("name").Value.ToString
                Else
                    .name = "Name-Unknown"
                End If

                
                If ADEntry.Properties("distinguishedname").Value IsNot Nothing Then
                    .distinguishedname = ADEntry.Properties("distinguishedname").Value.ToString
                Else
                    .distinguishedname = "DN-Unknown"
                End If
            End With

        Next
        ADResults.Dispose()
        ADSearcher.Dispose()
        ADEntry.Dispose()

    End Sub

End Class
