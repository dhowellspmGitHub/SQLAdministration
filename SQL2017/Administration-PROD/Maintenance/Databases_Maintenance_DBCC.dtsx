CAST((CASE [user_access] WHEN 0 THEN 1 ELSE 0 END) AS BIT) AS [is_multi_user_access],
CAST([is_read_only] AS BIT) as [is_read_only],  -- readonly databases can be backed up
CAST(CASE WHEN [state] = 0 THEN 1 ELSE 0 END AS BIT) AS [is_online],
CAST(([state_desc] + ';' + [user_access_desc]) AS VARCHAR(128)) AS [database_status],
CAST([is_in_standby] AS BIT) AS [is_in_standby], 
(CAST([is_published] AS INT) + CAST([is_subscribed] AS INT) + CAST([is_merge_published] AS INT) + CAST([is_distributor] AS INT)) AS [is_used_in_replication]
FROM [sys].[databases]
WHERE [database_id] &gt; 4
ORDER BY [name]</property>
