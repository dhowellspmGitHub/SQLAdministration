                      expressionType="Notify"
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">declare @p1 table (offlinedefragdatatypes nvarchar(128))
insert into @p1 (offlinedefragdatatypes)
values ('text')
insert into @p1 (offlinedefragdatatypes)
values ('ntext')
insert into @p1 (offlinedefragdatatypes)
values ('image')
insert into @p1 (offlinedefragdatatypes)
values ('FILESTREAM')
insert into @p1 (offlinedefragdatatypes)
values ('xml')


SELECT
[ic1].[object_id]
,[ic1].[index_id]
,SUM(CAST(st1.is_user_defined as INT)) AS is_user_defined_count
,SUM(edt3.excluded_datatype_count) AS excluded_datatype_count
FROM [sys].[index_columns] AS ic1
INNER JOIN [sys].[columns] AS c2
ON
[ic1].[object_id] = [c2].[object_id]
AND [ic1].[column_id] = [c2].[column_id]
LEFT OUTER JOIN [sys].[types] AS st1
ON c2.system_type_id = st1.system_type_id
AND c2.user_type_id = st1.user_type_id
LEFT OUTER JOIN
	(
	SELECT
	t1.[object_id]
	,SUM(ISNULL(edt2.is_excluded_data_type,0)) as excluded_datatype_count
	FROM [sys].[tables] as t1
	INNER JOIN [sys].[columns] as c1
	ON t1.[object_id] = c1.[object_id]
	LEFT OUTER JOIN 
		(
		SELECT 
		st2.system_type_id
		,st2.user_type_id
		,cast(1 as int) AS is_excluded_data_type
		FROM [sys].[types] AS st2
		INNER JOIN 
			(SELECT 
			system_type_id
			,CAST(1 as int) as is_excluded_data_type
			,CASE  WHEN EDT3.offlinedefragdatatypes is null THEN 0 ELSE 1 END AS EDTInd
			FROM sys.types AS ST3
			inner join @p1 AS EDT3
			ON ST3.name = EDT3.offlinedefragdatatypes
			) as udt2
		on st2.system_type_id = udt2.system_type_id
		) AS edt2
	ON c1.system_type_id = edt2.system_type_id
	and c1.user_type_id = edt2.user_type_id
	GROUP BY 
	t1.[object_id]
	) AS edt3
ON ic1.[object_id] = edt3.[object_id]
GROUP BY 
[ic1].[object_id]
,[ic1].[index_id]
ORDER BY
[ic1].[object_id]
,[ic1].[index_id]</property>
