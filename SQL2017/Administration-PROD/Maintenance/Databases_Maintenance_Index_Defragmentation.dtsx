'ScriptMain is the entry point class of the script.  Do not change the name, attributes,
'or parent of this class.
<Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute()> _
<System.CLSCompliantAttribute(False)> _
Partial Public Class ScriptMain
    Inherits Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
	Public Sub Main()
		' RO Variables: User::ErrorHandling_FailedIndexes_TotalCount,User::Indexes_Processed_Count,User::Indexes_TotalCount,$Project::Login_Name_02
		' RW Variables: User::Defragment_Summary_Comments,User::ErrorHandling_PersistentText
		Dim intFailedIndexesCount As Integer = CInt(Dts.Variables("User::ErrorHandling_FailedIndexes_TotalCount").Value.ToString)
		Dim intProcessedIndexesCount As Integer = CInt(Dts.Variables("User::Indexes_Processed_Count").Value.ToString)
		Dim intTotalIndexesInDatabase As Integer = CInt(Dts.Variables("User::Indexes_TotalCount").Value.ToString)
		Dim strProjectUserName02 As String = Dts.Variables("$Project::Login_Name_02").Value.ToString
		Dim strSummaryComments As String = Dts.Variables("User::Defragment_Summary_Comments").Value.ToString
		Dim intCompletedDatabasesCount As Integer
		' This script inspects the cummulative indexes processed and failed after the FELC Indexes completes
		' If there was a fatal error in the prior process, then the text for the summary table is updated/appended
		' If no fatal error directly fired by the FELC Indexes, then the next step is to compare the count of indexes processed with the total indexes with error counts (the cummulative/running totals).
		' Business rules dictate that if every index processed experienced an error, then this must increment the databases with an error for the later application of another business rule (if all databases processed had failures defragmenting all their indexes, then issue a fatal error).
		' The only method of evaluating this business rule is to compare the count of processed indexes (which is not necessarily the total number of indexes in the database) to the count of index defragmentation tasks with failures.  If the failures is greater than or equal to the total number of processed indexes, then increment the databases having failures cummulative total
		Dim strSourceName As String = "FELC Indexes"
		Dim strLocalErrorMessage As String = String.Empty

		' If there are no indexes in the database, then there can be no work to be performed and therefore no errors.  If there is at least one index, then proper error determination is required
		If intTotalIndexesInDatabase > 0 Then
			' If the index errors equal or exceed the index count, then increment the databases with errors
			If intFailedIndexesCount >= intTotalIndexesInDatabase Then
				intCompletedDatabasesCount = 0
				strLocalErrorMessage = "ERROR at task: " + strSourceName + ": The number of indexes experiencing errors (" + intFailedIndexesCount.ToString + ") is equal to or greater than the number of indexes processed (" + intProcessedIndexesCount.ToString + ").  This indicates a complete failure to perform maintenance on this specific database.  Please confirm the authority granted to the user:" + strProjectUserName02 + ".  Error information on each individual index and errors recorded in the index defragmentation history details table;"
			Else
				' Even if the index errors are less than the index count, if the errors are greater than 0, the summary table must be updated with this information
				If intFailedIndexesCount > 0 Then
					intCompletedDatabasesCount = 1
					strLocalErrorMessage = "ERROR at task: " + strSourceName + ": The number of indexes experiencing errors (" + intFailedIndexesCount.ToString + ") is equal to or greater than the number of indexes processed (" + intProcessedIndexesCount.ToString + ").  This indicates a partial failure to perform maintenance on this specific database.  Please review the error information on the failed indexes. Errors for each indexes is recorded in the index defragmentation history details table;"
				Else
					intCompletedDatabasesCount = 1
					strLocalErrorMessage = "All indexes meeting the minimum requirements were defragmented.  Please review the index summary and detail tables for information on the specific indexes defragmented and the actions taken;"
				End If
			End If
		Else
			intCompletedDatabasesCount = 1
			strLocalErrorMessage = "The database contains no indexes in any table.  As there are no indexes, no work was performed;"
		End If
		'User::Databases_ProcessCompletedCount,User::Defragment_Summary_Comments
		Dts.Variables("User::Databases_ProcessCompletedCount").Value = intCompletedDatabasesCount
		Dts.Variables("User::Defragment_Summary_Comments").Value = strSummaryComments + Trim(strLocalErrorMessage)

		Dts.TaskResult = ScriptResults.Success
	End Sub

#Region "ScriptResults declaration"
