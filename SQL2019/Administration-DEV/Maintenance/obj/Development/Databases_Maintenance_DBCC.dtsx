	Enum ScriptResults
		Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
		Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
	End Enum
	
   
    Public Sub Main()
		'RO Variables: System::ErrorDescription,System::SourceName,$Project::Database_Name_01,$Project::Server_Name_01
		'RW Variables: User::ErrorHandling_NotificationText

		Dim strPersistentText As String = String.Empty
		strPersistentText = "The package failed at the task:" & vbTab & Dts.Variables("System::SourceName").Value.ToString & vbCrLf & _
		 "Project parameters values:" & vbCrLf & _
		 "Server_Name_01:" & vbTab & Dts.Variables("$Project::Server_Name_01").Value.ToString & vbCrLf & _
