	   "Package Error Description:" & vbTab & Dts.Variables("System::ErrorDescription").Value.ToString

		Dts.Variables("User::ErrorHandling_NotificationText").Value = Left(strPersistentText, 4000)


		Dts.TaskResult = ScriptResults.Success
    End Sub

