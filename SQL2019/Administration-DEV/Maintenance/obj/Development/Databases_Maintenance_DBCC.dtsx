        <SQLTask:SqlTaskData
          SQLTask:Connection="{65929BB9-C634-4B8B-BB72-13C7E6080F2A}"
          SQLTask:SqlStatementSource="&#xA;IF NOT EXISTS (SELECT * FROM sys.tables AS t1 INNER JOIN sys.schemas AS s1 ON t1.schema_id = s1.schema_id  WHERE t1.name = 'DBCC_History_Summary' AND t1.type = 'U'  AND s1.name = 'dbo')&#xA;BEGIN&#xA;CREATE TABLE [dbo].[DBCC_History_Summary](&#xA;&#x9;[DBCC_Job_ID] [int] IDENTITY(1,1) NOT NULL,&#xA;&#x9;[ExecutioninstanceGUID] [uniqueidentifier] NOT NULL,&#xA;&#x9;[Server_Name] [varchar](128) NOT NULL,&#xA;&#x9;[Initiating_Server] [varchar](100) NULL,&#xA;&#x9;[Initiating_Job_Name] [varchar](100) NULL,&#xA;&#x9;[Process_All_Databases] [bit] NOT NULL,&#xA;&#x9;[Databases_ToExclude] [varchar](4000) NULL,&#xA;&#x9;[Databases_ToProcess] [varchar](4000) NULL,&#xA;&#x9;[DBCC_Job_Starttime] [datetime] NOT NULL,&#xA;&#x9;[DBCC_Job_Endtime] [datetime] NULL,&#xA;&#x9;[DBCC_Parameters_CommandOptions] [varchar](1000) NULL,&#xA;&#x9;[DBCC_Parameters_CommandType] [varchar](256) NOT NULL,&#xA;&#x9;[DBCC_Parameters_CommandWITHConditions] [varchar](256) NOT NULL,&#xA;&#x9;[DBCC_Parameters_MaxAgeOfCheckDB] [int] NOT NULL,&#xA;&#x9;[Databases_Total_Count] [int] NULL,&#xA;&#x9;[Databases_Completed_Count] [int] NULL,&#xA;&#x9;[Databases_Failed_Count] [int] NULL,&#xA;&#x9;[DBCC_Summary_Comments] [varchar](8000) NULL,&#xA; CONSTRAINT [PK_DBCC_History_Summary] PRIMARY KEY CLUSTERED &#xA;(&#xA;&#x9;[DBCC_Job_ID] ASC&#xA;)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON) ON [PRIMARY]&#xA;) ON [PRIMARY]&#xA;END&#xA;&#xA;IF NOT EXISTS (SELECT * FROM sys.tables AS t1 INNER JOIN sys.schemas AS s1 ON t1.schema_id = s1.schema_id  WHERE t1.name = 'DBCC_History_Detail' AND t1.type = 'U'  AND s1.name = 'dbo')&#xA;BEGIN&#xA;&#x9;CREATE TABLE [dbo].[DBCC_History_Detail](&#xA;&#x9;[DBCC_Job_ID] [int] NOT NULL,&#xA;&#x9;[database_ID] [int] NOT NULL,&#xA;&#x9;[database_name] [varchar](128) NOT NULL,&#xA;&#x9;[dbi_dbccLastKnownGood] [datetime] NULL,&#xA;&#x9;[database_accessible_ind] [bit] NOT NULL,&#xA;&#x9;[DBCC_Database_Job_Starttime] [datetime] NOT NULL,&#xA;&#x9;[DBCC_Database_Job_Endtime] [datetime] NULL,&#xA;&#x9;[DBCC_performed_ind] [bit] NULL,&#xA;&#x9;[DBCC_completed_ind] [bit] NULL,&#xA;&#x9;[DBCC_failed_ind] [bit] NULL,&#xA;&#x9;[DBCC_Detail_Comments] [varchar] (8000) NULL,&#xA;&#x9;[DBCC_Command_Used] [varchar] (8000) NULL&#xA;&#x9;) ON [PRIMARY]&#xA;&#xA;&#x9;ALTER TABLE [dbo].[DBCC_History_Detail]  WITH CHECK ADD  CONSTRAINT [FK_DBCC_History_Detail_DBCC_History_Summary_01] FOREIGN KEY([DBCC_Job_ID])&#xA;&#x9;REFERENCES [dbo].[DBCC_History_Summary] ([DBCC_Job_ID])&#xA;END&#xA;&#xA;&#xA;&#xA;IF NOT EXISTS (SELECT * FROM sys.tables AS t1 INNER JOIN sys.schemas AS s1 ON t1.schema_id = s1.schema_id  WHERE t1.name = 'DBCC_History_LogInfo' AND t1.type = 'U'  AND s1.name = 'dbo')&#xA;BEGIN&#xA;&#x9;CREATE TABLE [dbo].[DBCC_History_LogInfo](&#xA;&#x9;[DBCC_Job_ID] [int] NULL,&#xA;&#x9;[database_ID] [int] NULL,&#xA;&#x9;[database_name] [varchar](128) NULL,&#xA;&#x9;[LogDate] DATETIME,&#xA;&#x9;[ProcessInfo] VARCHAR(50),&#xA;&#x9;[DBCC_Command_Output] [varchar](8000) NULL&#xA;&#x9;) ON [PRIMARY]&#xA;END&#xA;&#xA;IF NOT EXISTS (SELECT * FROM [sys].[tables] AS t1 INNER JOIN sys.schemas AS s1 ON t1.schema_id = s1.schema_id  WHERE t1.name = 'DBCC_History_LogInfo_Temp' AND t1.type = 'U'  AND s1.name = 'dbo')&#xA;BEGIN&#xA;CREATE TABLE [dbo].[DBCC_History_LogInfo_Temp](&#xA;&#x9;[LogDate] DATETIME&#xA;&#x9;,[ProcessInfo] VARCHAR(50)&#xA;&#x9;,[DBCC_Command_Output] [varchar](8000) NULL&#xA;&#x9;) ON [PRIMARY]&#xA;&#x9;&#xA;END&#xA;ELSE&#xA;BEGIN&#xA;&#x9;DELETE FROM [dbo].[DBCC_History_LogInfo_Temp]&#xA;END&#xA;&#xA;--Update any NULL DBCC Job end date to account for interrupted or failed prior DBCC jobs&#xA;UPDATE [dbo].[DBCC_History_Summary]&#xA;SET [DBCC_Job_Endtime] = DATEADD(SECOND,1,[DBCC_Job_Starttime])&#xA;WHERE [DBCC_Job_Endtime] IS NULL&#xA;&#xA;UPDATE [dbo].[DBCC_History_Detail]&#xA;SET [DBCC_Database_Job_Endtime] = DATEADD(SECOND,1,[DBCC_Database_Job_Starttime])&#xA;,[DBCC_completed_ind] = 0&#xA;,[DBCC_failed_ind] = 1&#xA;WHERE [DBCC_Database_Job_Endtime] IS NULL&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
