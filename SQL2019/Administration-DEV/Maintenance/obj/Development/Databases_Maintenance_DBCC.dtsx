Imports System
Imports System.Data
Imports System.Math
Imports Microsoft.SqlServer.Dts.Pipeline.Wrapper
Imports Microsoft.SqlServer.Dts.Runtime.Wrapper

#End Region

' This is the class to which to add your code.  Do not change the name, attributes, or parent
' of this class.
<Microsoft.SqlServer.Dts.Pipeline.SSISScriptComponentEntryPointAttribute>
<CLSCompliant(False)>
Public Class ScriptMain
    Inherits UserComponent

    Dim strProcessDatabaseNames As String
    Dim strProcessDatabaseName As String
    Dim strExcludedDatabaseNames As String
    Dim strExcludedDatabaseName As String
    Dim i As Integer
    Public Overrides Sub PreExecute()
        MyBase.PreExecute()
        strExcludedDatabaseNames = Me.Variables.DBCCParametersDatabasesToExclude.ToString()
        strProcessDatabaseNames = Me.Variables.DBCCParametersDatabasesToProcess
        i = 0

    End Sub

    Public Overrides Sub PostExecute()
        MyBase.PostExecute()

    End Sub

    Public Overrides Sub CreateNewOutputRows()
        If Len(strProcessDatabaseNames) > 0 Then
            Dim arrDatabasesToProcess() As String = Split(strProcessDatabaseNames, ",", , CompareMethod.Text)
            For i = 0 To arrDatabasesToProcess.Length - 1
                strProcessDatabaseName = LTrim(RTrim(arrDatabasesToProcess(i)))
                DatabasesListBuffer.AddRow()
                DatabasesListBuffer.DatabaseName = strProcessDatabaseName.ToUpper.Trim
                DatabasesListBuffer.IsNamedExclusion = False
            Next
        End If

        If Len(strExcludedDatabaseNames) > 0 Then
            Dim arrDatabasesToExclude() As String = Split(strExcludedDatabaseNames, ",", , CompareMethod.Text)
            For i = 0 To arrDatabasesToExclude.Length - 1
                strExcludedDatabaseName = LTrim(RTrim(arrDatabasesToExclude(i)))
                DatabasesListBuffer.AddRow()
                DatabasesListBuffer.DatabaseName = strExcludedDatabaseName.ToUpper
                DatabasesListBuffer.IsNamedExclusion = True
            Next
        End If


    End Sub

End Class
]]></arrayElement>
