<System.CLSCompliantAttribute(False)> _
Partial Public Class ScriptMain
    Inherits Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
    Public Sub Main()
        '
        ' Add your code here
        'RO: System::SourceName
        'RW: User::DBCC_Database_Completed_Ind,User::DBCC_Database_Failed_Ind,User::DBCC_Detail_Comments
        Dim strSourceName As String = Dts.Variables("System::SourceName").Value.ToString
        Dim strDBCCComments As String = Dts.Variables("User::DBCC_Detail_Comments").Value.ToString
        Dim strLocalComments As String = String.Empty
        Dim bolDBCCCompleteInd As Boolean = True
        Dim bolDBCCFailedInd As Boolean = True
        strLocalComments = "FAILURE: " + strSourceName + ": Query obtaining the last known DBCC execution failed.  Please investigate cause and correct. If necessary, rerun package naming the failed database in the 'Databases to Process' parameter;"
        strDBCCComments = strLocalComments + strDBCCComments
        Dts.Variables("User::DBCC_Detail_Comments").Value = strDBCCComments
        Dts.Variables("User::DBCC_Database_Completed_Ind").Value = bolDBCCCompleteInd
        Dts.Variables("User::DBCC_Database_Failed_Ind").Value = bolDBCCFailedInd
        Dts.Events.FireWarning(1, strSourceName, strDBCCComments, String.Empty, 0)

        Dts.TaskResult = ScriptResults.Success
    End Sub

#Region "ScriptResults declaration"
    Enum ScriptResults
        Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
        Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
    End Enum

#End Region

