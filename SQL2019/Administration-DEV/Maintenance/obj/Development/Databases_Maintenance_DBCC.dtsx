                Name="ScriptMain.vb"
                Encoding="UTF8"><![CDATA[#Region "Imports"
Imports System
Imports System.Data
Imports System.Math
Imports Microsoft.SqlServer.Dts.Runtime
#End Region

'ScriptMain is the entry point class of the script.  Do not change the name, attributes,
'or parent of this class.
<Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute()> _
<System.CLSCompliantAttribute(False)> _
Partial Public Class ScriptMain
    Inherits Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
    Public Sub Main()
        ' RO: System::TaskName,User::Database_Name_02_local,User::DBCC_Parameters_CommandOptions,User::DBCC_Parameters_CommandType,User::DBCC_Parameters_CommandWITHConditions,User::DBCC_Parameters_MaxAgeOfCheckDB,User::DBCC_Summary_LastKnownGoodDBCCDt,User::Server_Name_02

        ' RW:User::DBCC_Command_TSQL,User::DBCC_Detail_Comments,User::TSQL_Execution_EndTime,User::TSQL_Execution_StartTime

        ' Description: Script task builds the TSQL statement containing the DBCC command with options to run
        ' First, override all options if the number of days since the last DBCC CHECKDB is greater than that set by the package variable and include all messages
        ' If under that limit, then read the DBCC command and options to use. Build the TSQL command used by the later tasks
        ' However, repair functions should not be used.  If any repair options are specified in the parameters, halt the process and mark the process as failed
        Dim strTaskName As String = Dts.Variables("System::TaskName").Value.ToString
        Dim strDatabaseName As String = Dts.Variables("User::Database_Name_02_local").Value.ToString()
        Dim strDBCCCmdOptionsVariable As String = Dts.Variables("User::DBCC_Parameters_CommandOptions").Value.ToString()
        Dim strDBCCCommandType As String = Dts.Variables("User::DBCC_Parameters_CommandType").Value.ToString()
        Dim strDBCCWithOptionsVariable As String = Dts.Variables("User::DBCC_Parameters_CommandWITHConditions").Value.ToString()
        Dim intMaxDaysSinceDBCCCHECKDB As Integer = CInt(Dts.Variables("User::DBCC_Parameters_MaxAgeOfCheckDB").Value.ToString())
        Dim dtLastGoodDBCCDate As Date = CDate(Dts.Variables("User::DBCC_Summary_LastKnownGoodDBCCDt").Value.ToString())
        Dim strServerName As String = Dts.Variables("User::Server_Name_02").Value.ToString()
        Dim strDBCCDetailComments As String = Dts.Variables("User::DBCC_Detail_Comments").Value.ToString()

        Dim intDaysDiff As Integer
        Dim strDBCCCmdType As String = String.Empty
        Dim strDBCCCmdOptions As String = String.Empty
        Dim strDBCCWithOptions As String = String.Empty
        Dim strDBCCTSQLStmt As String = "DBCC <DBCCCmd> (<databasename> <DBCCCmdOptions> <DBCCWithOptions>"

        ' Declare variables for error handling and logging purposes
        Dim bolRunAlternateSQL As Boolean = False
        Dim intInternalErrorCount As Integer = 0
        Dim bolDTSTaskResults As DTSExecResult = DTSExecResult.Success
        Dim strProcessMessage As String = String.Empty
        Dim strExceptionMessage As String = String.Empty

        Dim intErrorSeverityLevel As Integer = 0
        Dim bolObjectErrorInd As Boolean = False

        Try
            ' First, validate options and DBCC command--this performs no validation that proper TSQL syntax is used or validation of the DBCC commands against known types

            ' If no command options provided, then close the DBCC command string
            If strDBCCCmdOptionsVariable.Length = 0 Then
                strDBCCCmdOptions = ")"
            Else
                strDBCCCmdOptions = strDBCCCmdOptionsVariable + ")"
            End If

            ' If no WITH options included, set the string value.  Otherwise use the parameter
            If strDBCCWithOptionsVariable.Length > 0 Then
                strDBCCWithOptions = "WITH " + strDBCCWithOptionsVariable
            End If

            intDaysDiff = CInt(DateDiff(DateInterval.Day, dtLastGoodDBCCDate, Today.Date))
            If intDaysDiff >= intMaxDaysSinceDBCCCHECKDB Then
                strDBCCCommandType = "CHECKDB"
                strDBCCCmdOptions = ")"
                strDBCCWithOptions = "WITH ALL_ERRORMSGS"
                strProcessMessage = "OVERRIDE-Days since last CHECKDB greater than maximum defined by parameter; "
            Else
                strProcessMessage = "Using command type and options in parameters; "
            End If

            strDBCCTSQLStmt = Replace(strDBCCTSQLStmt, "<DBCCCmd>", strDBCCCommandType, 1, -1, CompareMethod.Text)
            strDBCCTSQLStmt = Replace(strDBCCTSQLStmt, "<databasename>", strDatabaseName, 1, -1, CompareMethod.Text)
            strDBCCTSQLStmt = Replace(strDBCCTSQLStmt, "<DBCCCmdOptions>", strDBCCCmdOptions, 1, -1, CompareMethod.Text)
            strDBCCTSQLStmt = Replace(strDBCCTSQLStmt, "<DBCCWithOptions>", strDBCCWithOptions, 1, -1, CompareMethod.Text)

            strProcessMessage = strProcessMessage

            If strDBCCCmdOptions.ToUpper.Contains("REPAIR") = True Then
                strProcessMessage = strProcessMessage + "DISALLOWED-Repair options must only be used by a SQL DBA in live session"
                Throw New Exception(strProcessMessage)
            End If


        Catch ex As Exception
            ' Capture the entire exception message to a string and write the message to the SSIS package log as an error.
            'strProcessMessage = strProcessMessage & "User Name:" & strUserName & "--SMO Connection failed;" & vbCrLf
            ' RW:User::DBCC_Command_TSQL,User::DBCC_Detail_Comments,User::TSQL_Execution_EndTime,User::TSQL_Execution_StartTime
            Dim strInnerMsg As String = "Inner Exception:"
            Do While ex.InnerException IsNot Nothing
                strInnerMsg = strInnerMsg & ex.InnerException.Message.ToString
                ex = ex.InnerException
            Loop
            strExceptionMessage = ex.Message + vbTab + strInnerMsg
            bolDTSTaskResults = DTSExecResult.Failure
            Dts.Events.FireError(0, strTaskName, "Server Name:" & strServerName & "--" & strExceptionMessage, String.Empty, 0)
        Finally
            Dts.Variables("User::DBCC_Command_TSQL").Value = strDBCCTSQLStmt
            Dts.Variables("User::DBCC_Detail_Comments").Value = strProcessMessage + "; " + strDBCCDetailComments
            Dts.Variables("User::TSQL_Execution_StartTime").Value = Now()
            Dts.Variables("User::TSQL_Execution_EndTime").Value = Now()
        End Try


        Dts.TaskResult = bolDTSTaskResults
    End Sub

#Region "ScriptResults declaration"
    'This enum provides a convenient shorthand within the scope of this class for setting the
    'result of the script.

    'This code was generated automatically.
    Enum ScriptResults
        Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
        Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
    End Enum

#End Region

End Class]]></ProjectItem>
