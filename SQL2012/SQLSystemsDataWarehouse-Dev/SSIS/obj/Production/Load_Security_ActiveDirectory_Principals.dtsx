                                    .description = ADUser.Properties("description").Value.ToString
                                Else
                                    .description = String.Empty
                                End If
                            End If

                            If ADUser.Properties.Contains("displayname") = True Then
                                If ADUser.Properties("displayname").Value IsNot Nothing Then
                                    .displayname = ADUser.Properties("displayname").Value.ToString
                                Else
                                    .displayname = String.Empty
                                End If
                            End If


                            If ADUser.Properties.Contains("whenchanged") = True Then
                                If ADUser.Properties("whenchanged").Value IsNot Nothing Then
                                    .whenchanged = ADUser.Properties("whenchanged").Value.ToString
                                Else
                                    .whenchanged = CDate("01/01/1900")
                                End If
                            End If

                            If ADUser.Properties.Contains("whencreated") = True Then
                                If ADUser.Properties("whencreated").Value IsNot Nothing Then
                                    .whencreated = ADUser.Properties("whencreated").Value.ToString
                                Else
                                    .whencreated = CDate("01/01/1900")
                                End If
                            End If

                            If ADUser.Properties.Contains("distinguishedname") = True Then
                                If ADUser.Properties("distinguishedname").Value IsNot Nothing Then
                                    .distinguishedname = ADUser.Properties("distinguishedname").Value.ToString
                                Else
                                    .distinguishedname = String.Empty
                                End If
                            End If


                            If ADUser.Name.ToString IsNot Nothing Then
                                .name = ADUser.Name.ToString
                            Else
                                .name = String.Empty
                            End If

                            If ADUser.Guid.ToString IsNot Nothing Then
                                .guid = ADUser.Guid
                            Else
                                .guid = Guid.Empty
                            End If

                            If ADUser.Properties.Contains("samaccountname") = True Then
                                If ADUser.Properties("samaccountname").Value IsNot Nothing Then
                                    .samaccountname = ADUser.Properties("samaccountname").Value.ToString
                                Else
                                    .samaccountname = String.Empty
                                End If
                            End If

                            If ADUser.Properties.Contains("givenname") = True Then
                                If ADUser.Properties("givenname").Value IsNot Nothing Then
                                    .givenname = ADUser.Properties("givenname").Value.ToString
                                Else
                                    .givenname = String.Empty
                                End If
                            End If

                            If ADUser.Properties.Contains("sn") = True Then
                                If ADUser.Properties("sn").Value IsNot Nothing Then
                                    .sn = ADUser.Properties("sn").Value.ToString
                                Else
                                    .sn = String.Empty
                                End If
                            End If

                            If ADUser.Properties.Contains("manager") = True Then
                                If ADUser.Properties("manager").Value IsNot Nothing Then
                                    .manager = ADUser.Properties("manager").Value.ToString
                                Else
                                    .manager = String.Empty
                                End If
                            End If

                            If ADUser.Properties.Contains("title") = True Then
                                If ADUser.Properties("title").Value IsNot Nothing Then
                                    .title = ADUser.Properties("title").Value.ToString
                                Else
                                    .title = String.Empty
                                End If
                            End If

                            If ADUser.Properties.Contains("department") = True Then
                                If ADUser.Properties("department").Value.ToString IsNot Nothing Then
                                    .department = ADUser.Properties("department").Value.ToString
                                Else
                                    .department = String.Empty
                                End If
                            Else
                                .department = String.Empty
                            End If


                            If ADUser.Properties.Contains("userAccountControl") = True Then
                                If ADUser.Properties("userAccountControl").Value IsNot Nothing Then
                                    .useraccountcontrol = ADUser.Properties("userAccountControl").Value
                                    .userAccountDisabledInd = CBool(Mid(Convert.ToString(CInt(ADUser.Properties("userAccountControl").Value.ToString), 2).PadLeft(32, "0"), 31, 1))
                                Else
                                    .useraccountcontrol = 0
                                    .userAccountDisabledInd = False
                                End If
                            Else
                                .useraccountcontrol = 0
                                .userAccountDisabledInd = False
                            End If

                            i = i + 1
                        End With

                        ADUser.Dispose()
                    Next
                    Me.ComponentMetaData.FireInformation(0, "Load AD Users", "Total Users Count=" + j.ToString + vbTab + "Users Loaded to table=" + i.ToString, String.Empty, 0, False)
                    ADUsersResults.Dispose()
                End If
                CustomLogText = AdsPath.ToString + "--This is logging but not an exception. Contains no users"
                Me.ComponentMetaData.FireInformation(0, Me.Variables.TaskName.ToString, CustomLogText, String.Empty, 0, False)
            End If
        Catch ex As Exception
            CustomLogText = AdsPath.ToString + "--This is the Exception firing. Contains no users"
