                                If ADUser.Properties("distinguishedname").Value IsNot Nothing Then
                                    .distinguishedname = ADUser.Properties("distinguishedname").Value.ToString
                                Else
                                    .distinguishedname = String.Empty
                                End If
                            End If


                            If ADUser.Name.ToString IsNot Nothing Then
                                .name = ADUser.Name.ToString
                            Else
                                .name = String.Empty
                            End If

                            If ADUser.Guid.ToString IsNot Nothing Then
                                .guid = ADUser.Guid
                            Else
                                .guid = Guid.Empty
                            End If

                            If ADUser.Properties.Contains("samaccountname") = True Then
                                If ADUser.Properties("samaccountname").Value IsNot Nothing Then
                                    .samaccountname = ADUser.Properties("samaccountname").Value.ToString
                                Else
                                    .samaccountname = String.Empty
                                End If
                            End If

                            If ADUser.Properties.Contains("givenname") = True Then
                                If ADUser.Properties("givenname").Value IsNot Nothing Then
                                    .givenname = ADUser.Properties("givenname").Value.ToString
                                Else
                                    .givenname = String.Empty
                                End If
