    Dim ADUsersResults As SearchResultCollection
    Dim ADUsersEntry As DirectoryEntry
    Dim ADUsersSearcher As DirectorySearcher
    Dim CustomLogText As String = String.Empty
    Dim AdsPath As String
    Dim i As Integer = 0
    Dim j As Integer = 0
    Dim x As String()
    Dim y As String

    Public Overrides Sub PreExecute()
        MyBase.PreExecute()
        Try
            AdsPath = Me.Variables.ADObjectAdsPath
            'ADUsersEntry = New DirectoryEntry(AdsPath, "KFBDOM1\DXHADMIN", "PEN1865cil")
            ADUsersEntry = New DirectoryEntry(AdsPath)
            ADUsersSearcher = New DirectorySearcher(ADUsersEntry)
            ADUsersSearcher.Filter = "(&(objectCategory=person)(objectClass=user))"
            'ADUsersSearcher.Filter = "*(&(objectClass=user))"
            ADUsersSearcher.SearchScope = SearchScope.OneLevel
            ADUsersSearcher.PageSize = 2000
            ADUsersSearcher.ClientTimeout = New TimeSpan(0, 5, 0)
            ADUsersResults = ADUsersSearcher.FindAll()
        Catch ex As Exception
            Dim strInnerMsg As String = "Exception firing at PreExecute for--" + AdsPath.ToString
            Dim exInnerException As Exception
            Do While ex.InnerException IsNot Nothing
                strInnerMsg = strInnerMsg + ex.InnerException.Message.ToString
                exInnerException = ex.InnerException
            Loop
            Me.ComponentMetaData.FireWarning(0, Me.Variables.TaskName.ToString, strInnerMsg.ToString, String.Empty, 0)
        End Try

    End Sub
    Public Overrides Sub CreateNewOutputRows()
        Try
            If ADUsersResults IsNot Nothing Then
                j = ADUsersResults.Count
                If ADUsersResults.Count > 0 Then
                    For Each ADSearchResult As SearchResult In ADUsersResults
                        Dim ADUser As DirectoryEntry
                        ADUser = ADSearchResult.GetDirectoryEntry()
                        With VBSADUsersBuffer
                            .AddRow()
                            Dim EntrySID As Byte() = DirectCast(ADUser.Properties("objectSid").Value, Byte())
                            .sid = EntrySID

                            If ADUser.Properties.Contains("cn") = True Then
                                If ADUser.Properties("cn").Value IsNot Nothing Then
                                    .cn = ADUser.Properties("cn").Value.ToString
                                Else
                                    .cn = String.Empty
                                End If
                            End If

                            If ADUser.Properties.Contains("objectcategory") = True Then
