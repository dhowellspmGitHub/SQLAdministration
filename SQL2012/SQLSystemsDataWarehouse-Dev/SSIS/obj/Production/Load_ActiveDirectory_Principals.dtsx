                                Else
                                    .objectcategory = String.Empty
                                End If
                            End If

                            If ADUser.Properties.Contains("userprincipalname") = True Then
                                If ADUser.Properties("userprincipalname").Value IsNot Nothing Then
                                    .userprincipalname = ADUser.Properties("userprincipalname").Value.ToString
                                Else
                                    .userprincipalname = String.Empty
                                End If
                            End If

                            If ADUser.Properties.Contains("samaccounttype") = True Then
                                If ADUser.Properties("samaccounttype").Value IsNot Nothing Then
                                    .samaccounttype = ADUser.Properties("samaccounttype").Value
                                Else
                                    .samaccounttype = 0
                                End If
                            End If


