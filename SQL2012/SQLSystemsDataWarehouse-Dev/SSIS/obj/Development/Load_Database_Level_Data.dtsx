Imports System
Imports System.Data
Imports System.Math
Imports Microsoft.SqlServer.Dts.Runtime
#End Region

'ScriptMain is the entry point class of the script.  Do not change the name, attributes,
'or parent of this class.
<Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute()> _
<System.CLSCompliantAttribute(False)> _
Partial Public Class ScriptMain
    Inherits Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
    Public Sub Main()
        'RO: User::ErrorHandling_NULLAppIDCount,User::ErrorHandling_NULLAppSeqIDCount,System::ExecutionInstanceGUID,System::PackageName,$Package::InitiatingJobName,$Project::Database_Name_01
        'RW: User::Email_Source,User::Email_Subject,User::ErrorHandling_SendEmail
        Dim strPackageName As String = Dts.Variables("System::PackageName").Value.ToString
        Dim strInitiatingJobName As String = Dts.Variables("$Package::InitiatingJobName").Value.ToString
        Dim strExecutionID As String = Dts.Variables("System::ExecutionInstanceGUID").Value.ToString
        Dim strEmailFooter As String = "Package Name: " + strPackageName + vbCrLf + "Job Name: " + strInitiatingJobName + vbCrLf + "Execution: " + strExecutionID
        Dim strDatabaseName01 As String = Dts.Variables("$Project::Database_Name_01").Value.ToString
        Dim intAppIDErrorCount As Integer = CInt(Dts.Variables("User::ErrorHandling_NULLAppIDCount").Value.ToString)
        Dim intAppSeqIDErrorCount As Integer = CInt(Dts.Variables("User::ErrorHandling_NULLAppSeqIDCount").Value.ToString)

        Dim bolSendEmail As Boolean = False
        Dim strEmailSource As String = String.Empty
        Dim strDatabasesErrorText As String = String.Empty
        Dim strPackageCompletionState As String = "WARNINGS"


        Dim intTotalErrorsCount As Integer = intAppIDErrorCount + intAppSeqIDErrorCount
        'If others are notified, include SQL Admins in notice of failure.  If no one is notified, at least notify SQL admins
        If Len(Dts.Variables("User::Email_ToLine").Value.ToString) = 0 Then
            Dts.Variables("User::Email_ToLine").Value = "sql_alerts@kyfb.com"
        End If
        intTotalErrorsCount = intAppIDErrorCount + intAppSeqIDErrorCount
        If intTotalErrorsCount > 0 Then
            bolSendEmail = True
            strDatabasesErrorText = "Database Extended Properties:" + vbCrLf + _
            "Errors indicate either:" + vbCrLf + _
            "1) new databases were found for which an ApplicationID could not be assigned" + vbCrLf + _
            "2) at least one database name has been assigned more than one application or," + vbCrLf + _
            "3) at least one database has been assigned two different ApplicationDatabaseSeqID" + vbCrLf + _
            "Error counts respectively are:" + vbCrLf + _
            "AppID Results Count: " + intAppIDErrorCount.ToString + vbCrLf + _
            "AppSeqID Results Count: " + intAppSeqIDErrorCount.ToString + vbCrLf + _
            "Please manually run the ApplicationID and/or Application Sequence ID assignment stored procedures in the " + _
            strDatabaseName01 + " database." + vbCrLf
        End If


        Dts.Variables("User::Email_Subject").Value = strPackageCompletionState + "--" + strPackageName
        strEmailSource = "The package returned: " + LCase(strPackageCompletionState) + vbCrLf + strDatabasesErrorText + _
        "Summary and detail logging tables are in the SSIS Execution Catalog viewable using the Integration Service Catalogs--Reports" + vbCrLf + _
        vbCrLf + strEmailFooter

        Dts.Variables("User::Email_Source").Value = Left(Trim(strEmailSource), 4000)
        Dts.Variables("User::ErrorHandling_SendEmail").Value = bolSendEmail

        Dts.TaskResult = ScriptResults.Success
    End Sub

#Region "ScriptResults declaration"
    'This enum provides a convenient shorthand within the scope of this class for setting the
    'result of the script.

    'This code was generated automatically.
    Enum ScriptResults
        Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
        Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
    End Enum

#End Region

End Class]]></ProjectItem>
