                Name="ScriptMain.vb"
                Encoding="UTF8"><![CDATA[#Region "Help:  Introduction to the script task"
'The Script Task allows you to perform virtually any operation that can be accomplished in
'a .Net application within the context of an Integration Services control flow. 

'Expand the other regions which have "Help" prefixes for examples of specific ways to use
'Integration Services features within this script task.
#End Region


#Region "Imports"
Imports System
Imports System.Data
Imports System.Math
Imports Microsoft.SqlServer.Dts.Runtime
#End Region

'ScriptMain is the entry point class of the script.  Do not change the name, attributes,
'or parent of this class.
<Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute()> _
<System.CLSCompliantAttribute(False)> _
Partial Public Class ScriptMain
    Inherits Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase

	Public Sub Main()
		' RO Variables: User::Indexes_TotalCount,$Project::Login_Name_02
		' RW Variables: User::Databases_ProcessCompletedCount,User::Defragment_Summary_Comments,User::ErrorHandling_FailedDatabases_Count,User::ErrorHandling_FailedIndexes_TotalCount,User::ErrorHandling_NonFatalErrors_Count,User::ErrorHandling_PersistentText,User::Indexes_Processed_Count


		Dim intRunningFECount As Integer = CInt(Dts.Variables("User::ErrorHandling_FatalErrors_Count").Value.ToString)
		Dim intRunningNFECount As Integer = CInt(Dts.Variables("User::ErrorHandling_NonFatalErrors_Count").Value.ToString)
		Dim intProcessedDatabasesCount As Integer = CInt(Dts.Variables("User::Databases_ProcessCompletedCount").Value.ToString)
		Dim intRunningDatabasesWithErrorsCount As Integer = CInt(Dts.Variables("User::ErrorHandling_FailedDatabases_Count").Value.ToString)
		Dim intRunningFailedIndexesCount As Integer = CInt(Dts.Variables("User::ErrorHandling_FailedIndexes_TotalCount").Value.ToString)
		Dim intRunningProcessedIndexesCount As Integer = CInt(Dts.Variables("User::Indexes_Processed_Count").Value.ToString)
		Dim intTotalIndexesInDatabase As Integer = CInt(Dts.Variables("User::Indexes_TotalCount").Value.ToString)


		' This script inspects the cummulative indexes processed and failed after the FELC Indexes completes
		' If there was a fatal error in the prior process, then the text for the summary table is updated/appended
		' If no fatal error directly fired by the FELC Indexes, then the next step is to compare the count of indexes processed with the total indexes with error counts (the cummulative/running totals).
		' Business rules dictate that if every index processed experienced an error, then this must increment the databases with an error for the later application of another business rule (if all databases processed had failures defragmenting all their indexes, then issue a fatal error).
		' The only method of evaluating this business rule is to compare the count of processed indexes (which is not necessarily the total number of indexes in the database) to the count of index defragmentation tasks with failures.  If the failures is greater than or equal to the total number of processed indexes, then increment the databases having failures cummulative total
		Dim strSourceName As String = "FELC Indexes"
		Dim strLocalErrorMessage As String = Dts.Variables("User::ErrorHandling_PersistentText").Value.ToString

		' If there are no indexes in the database, then there can be no work to be performed and therefore no errors.  If there is at least one index, then proper error determination is required
		If intTotalIndexesInDatabase > 0 Then
			If (intRunningFECount > 0) Then
				strLocalErrorMessage = strLocalErrorMessage + vbCrLf + "ERROR at task: " + strSourceName + ": Error processing at least one index which may have resulted in a fatal error to the package. Please confirm the password and authority granted to the user:" + Dts.Variables("$Project::Login_Name_02").Value.ToString + ".  Error information on each individual index and errors recorded in the index defragmentation history details table."
			Else
				intProcessedDatabasesCount = intProcessedDatabasesCount + 1

				' If the index errors equal or exceed the index count, then increment the databases with errors
				If intRunningFailedIndexesCount >= intTotalIndexesInDatabase Then
					intRunningDatabasesWithErrorsCount = intRunningDatabasesWithErrorsCount + 1
					intRunningNFECount = intRunningNFECount + 1
					strLocalErrorMessage = strLocalErrorMessage + vbCrLf + "ERROR at task: " + strSourceName + ": The number of indexes experiencing errors (" + intRunningFailedIndexesCount.ToString + ") is equal to or greater than the number of indexes processed (" + intRunningProcessedIndexesCount.ToString + ").  This indicates a complete failure to perform maintenance on this specific database.  Please confirm the authority granted to the user:" + Dts.Variables("$Project::Login_Name_02").Value.ToString + ".  Error information on each individual index and errors recorded in the index defragmentation history details table."

				Else
					' Even if the index errors are less than the index count, if the errors are greater than 0, the summary table must be updated with this information
					If intRunningFailedIndexesCount > 0 Then
						intRunningNFECount = intRunningNFECount + 1
						strLocalErrorMessage = strLocalErrorMessage + vbCrLf + "ERROR at task: " + strSourceName + ": The number of indexes experiencing errors (" + intRunningFailedIndexesCount.ToString + ") is equal to or greater than the number of indexes processed (" + intRunningProcessedIndexesCount.ToString + ").  This indicates a partial failure to perform maintenance on this specific database.  Please review the error information on the failed indexes. Errors for each indexes is recorded in the index defragmentation history details table."
					Else
						strLocalErrorMessage = strLocalErrorMessage + vbCrLf + "All indexes meeting the minimum requirements were defragmented.  Please review the index summary and detail tables for information on the specific indexes defragmented and the actions taken."
					End If

				End If
			End If
		Else
			intProcessedDatabasesCount = intProcessedDatabasesCount + 1
			strLocalErrorMessage = strLocalErrorMessage + vbCrLf + "The database contains no indexes in any table.  As there are no indexes, no work was performed."
		End If

		Dts.Variables("User::ErrorHandling_FatalErrors_Count").Value = intRunningFECount
		Dts.Variables("User::ErrorHandling_NonFatalErrors_Count").Value = intRunningNFECount
		Dts.Variables("User::ErrorHandling_FailedDatabases_Count").Value = intRunningDatabasesWithErrorsCount
		Dts.Variables("User::Databases_ProcessCompletedCount").Value = intProcessedDatabasesCount
		Dts.Variables("User::Defragment_Summary_Comments").Value = Trim(strLocalErrorMessage)

		Dts.TaskResult = ScriptResults.Success
	End Sub

#Region "ScriptResults declaration"
    'This enum provides a convenient shorthand within the scope of this class for setting the
    'result of the script.

    'This code was generated automatically.
    Enum ScriptResults
        Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
        Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
    End Enum

#End Region

End Class]]></ProjectItem>
