	Public Sub Main()
        ' Script Notes, Information and Description:
        ' Index should be rebuild when index fragmentation is higher.  Index should be reorganized when index fragmentation is between an acceptible percentage and the threshold at which a rebuild is done.  Index rebuilding process uses more CPU and it locks the database resources. SQL Server development version and Enterprise version has option ONLINE, which can be turned on when Index is rebuilt. ONLINE option will keep index available during the rebuilding.
        ' Index Rebuild : This process drops the existing Index and Recreates the index.  Done when fragmentation is greater than 40%.
        ' Index Reorganize : This process physically reorganizes the leaf nodes of the index.  Done when fragmentation is between 10% to 40%.

        ' There are three criteria on determining if an index is defragmented
        ' 1) Was the index rebuilt recently-Defragmentation is a costly process and to prevent excessive processing, the package does not defragment indexes that have been defragmented with a minimum number of days regardless of how fragmented it is--User::Index_Defragment_AgeRequirement,
        ' 2) Minimum size--defragmentation of small indexes will not be performed regardless of the percentage fragmented--to eliminate their processing, an index must have a minimum number of pages (size)--User::Index_Defragment_Minimum_PageCount
        '3) Minimum Fragmentation--defragmentation of indexes will not be performed if the current percent fragmented is below a certain level--there is no real gain in performance or space--to eliminate their processing, an index must have fragmentation over a threshold--User::Index_Reorganize_Fragmentation_Percent

        ' If it is determined that an index should be defragmented, there are two factors to account for in determining the method of defragmentation
        ' A) SQL Server Edition--only SQL Server Enterprise edition can defragment and index while online; all other SQL Editions must take the index offline to defragment them--determine SQL Edition retrieved in an earlier task
        ' B) Rebuild or Reorganize the Index--For highly fragmented indexes, it is more economical to rebuild the index than to reorganize it--if the fragmentation is above a threshold, rebuild it otherwise reorganize the index -- User::Index_Rebuild_Fragementation_Percent


        ' Input package variables
        ' RO Variables: System::TaskName,User::Database_Name_02_local,User::Defragment_Parameters_AgeRequirement,User::Defragment_Parameters_Fragmentation_Percent_Threshold,User::Defragment_Parameters_Minimum_PageCount,User::Defragment_Parameters_OverrideOnlineOps,User::Defragment_Parameters_Rebuild_Percent_Threshold,User::Index_Name,User::Index_Stats_Excluded_DataType_Count,User::Index_Stats_Index_Type,User::Index_Stats_UserDef_Column_Count,User::IndexHistory_DefragSummaryDate,User::Schema_Name,User::SMO_ConnectionOverride,User::SMO_ConnectionTimeout_local,User::SMO_StatementTimeout_local,User::Table_Name,$Project::Server_Name_02,$Project::Login_Name_02,$Project::Login_Password_02

        ' RW Variables: User::Defragment_Detail_Comments,User::ErrorHandling_FailedIndexes_TotalCount,User::ErrorHandling_FatalErrors_Count,User::ErrorHandling_NonFatalErrors_Count,User::ErrorHandling_PersistentText,User::Index_Stats_Fragmentation_Percent,User::Index_Stats_Fragmentation_Percent_PreRun,User::Index_Stats_Page_Count,User::Index_Stats_Page_Count_PreRun,User::Indexes_Processed_Count,User::IndexHistory_DefragmentFailedInd,User::IndexHistory_IndexDefragEndDate,User::IndexHistory_IndexDefragStartDate,User::IndexHistory_Most_Recent_Defragmented_Date,User::IndexHistory_PriorDefragmentPerformed_Ind,User::IndexHistory_Rebuild_PerformedbyPriorJob_Ind,User::IndexHistory_ReorgPerformedbyPriorJob_Ind,User::Table_RowCount

        'error handling
        Dim strEventLogDesc As String = String.Empty
		Dim strEventSubComp As String = Dts.Variables("System::TaskName").Value.ToString
		Dim strTaskName As String = Dts.Variables("System::TaskName").Value.ToString
		Dim bolDTSTaskResults As DTSExecResult = DTSExecResult.Success
		'Dim bolLocalFatalErrorInd As Boolean = True


		Dim intRunningFECount As Integer = CInt(Dts.Variables("User::ErrorHandling_FatalErrors_Count").Value.ToString)
		Dim intRunningNFECount As Integer = CInt(Dts.Variables("User::ErrorHandling_NonFatalErrors_Count").Value.ToString)
        Dim strExceptionMessage As String = String.Empty
		Dim intRunningFailedIndexesCount As Integer = CInt(Dts.Variables("User::ErrorHandling_FailedIndexes_TotalCount").Value.ToString)
		Dim intRunningProcessedIndexesCount As Integer = CInt(Dts.Variables("User::Indexes_Processed_Count").Value.ToString)
		Dim bolDefragmentationFailedInd As Boolean = True

		' Set local variables for package variables
		Dim strServerName As String = Dts.Variables("$Project::Server_Name_02").Value.ToString
		Dim strUserName As String = Dts.Variables("$Project::Login_Name_02").Value.ToString
		Dim strDatabaseName As String = Dts.Variables("User::Database_Name_02_local").Value.ToString 'RO
		Dim strSchemaName As String = Dts.Variables("User::Schema_Name").Value.ToString	'RO
		Dim strTableName As String = Dts.Variables("User::Table_Name").Value.ToString 'RO
		Dim strIndexName As String = Dts.Variables("User::Index_Name").Value.ToString 'RO
        Dim intIndexType As Integer = CInt(Dts.Variables("User::Index_Stats_Index_Type").Value.ToString)

		Dim dblMinimumFragmenationParameter As Double = CDbl(Dts.Variables("User::Defragment_Parameters_Fragmentation_Percent_Threshold").Value.ToString) 'RO
		Dim intMinPageCountParameter As Int64 = CInt(Dts.Variables("User::Defragment_Parameters_Minimum_PageCount").Value.ToString)	'RO
		Dim intMinDefragDateDiffParameter As Int32 = CInt(Dts.Variables("User::Defragment_Parameters_AgeRequirement").Value.ToString) 'RO
		Dim dtDefragJobStartDate As Date = CDate(Dts.Variables("User::IndexHistory_DefragSummaryDate").Value.ToString)
        Dim dblCurrentFragmentationPercent As Double = CDbl(Dts.Variables("User::Index_Stats_Fragmentation_Percent_PreRun").Value.ToString) 'RW
        Dim intCurrentPageCount As Int32 = CInt(Dts.Variables("User::Index_Stats_Page_Count_PreRun").Value.ToString)    'RW
        Dim dtLastDefragDate As Date = CDate(Dts.Variables("User::IndexHistory_Most_Recent_Defragmented_Date").Value.ToString) 'RW
		Dim intCustomDataTypeCounter As Integer = CInt(Dts.Variables("User::Index_Stats_UserDef_Column_Count").Value.ToString)
		Dim intExcludedDataTypesCounter As Integer = CInt(Dts.Variables("User::Index_Stats_Excluded_DataType_Count").Value.ToString)
		Dim dblRebuildIndexThreshold As Double = CDbl(Dts.Variables("User::Defragment_Parameters_Rebuild_Percent_Threshold").Value.ToString)


		' Set local variable for script only
		Dim intDaysDifferent As Long = DateDiff(DateInterval.Day, dtLastDefragDate, dtDefragJobStartDate)
		Dim strServerEdition As String
		Dim bolOnlineAvailable As Boolean = False
		Dim dtIndexDefragStartTime As DateTime = Now()
		Dim dtIndexDefragEndTime As DateTime
		Dim bolIndexReorgPerformed As Boolean = False
		Dim bolIndexRebuildPerformed As Boolean = False
		Dim bolIndexDefragPerformed As Boolean = False
		Dim intTableRowCount As Int64
		Dim strIndexDetailComments As String = String.Empty

		'SMO 
		Dim connSMO As New ServerConnection()
		Dim objServer As Microsoft.SqlServer.Management.Smo.Server
		Dim objDatabase As Microsoft.SqlServer.Management.Smo.Database
		Dim objTable As Microsoft.SqlServer.Management.Smo.Table
		Dim objIndex As Microsoft.SqlServer.Management.Smo.Index

		Try
			connSMO.LoginSecure = False
			connSMO.Login = strUserName
			connSMO.ServerInstance = strServerName
			connSMO.Password = Dts.Variables("$Project::Login_Password_02").GetSensitiveValue().ToString
			If CBool(Dts.Variables("User::SMO_ConnectionOverride").Value.ToString) = True Then
				connSMO.ConnectTimeout = CInt(Dts.Variables("User::SMO_ConnectionTimeout_local").Value.ToString)
				connSMO.StatementTimeout = CInt(Dts.Variables("User::SMO_StatementTimeout_local").Value.ToString)
			End If
			objServer = New Microsoft.SqlServer.Management.Smo.Server(connSMO)
            'The inner try will not fail on an invalid server connection--set up an outer Try Catch block to prevent the package from proceeding if there is a problem connecting to the server
            Dim strConnInformation As String = String.Empty
			strServerEdition = objServer.EngineEdition.ToString
            strConnInformation = "SMO Connection Properties to: " & strServerName & vbTab & "SMO Connection Timeout:" & connSMO.ConnectTimeout.ToString & vbTab & "; SMO Statement Timeout: " & vbTab & connSMO.StatementTimeout.ToString

            objDatabase = objServer.Databases(strDatabaseName)
            objTable = objDatabase.Tables(strTableName, strSchemaName)
            intTableRowCount = objTable.RowCount()
           

			' Index defragmentation can be done ONLINE only for the SQL Enterprise edition.  However, even if the edition is Enterprise, index operations fail if the index contains custom (user defined) data types or if specific data types are used in the index (as either component columns or included columns).  The prior DFT task loading Index statistics includes tasks/methods gathering information on whether the index contains these data types.  Any number greater than 0 indicates the index rebuild/reorg cannot occur online.
			If (InStr(1, UCase(strServerEdition), "ENTERPRISE", CompareMethod.Text) > 0) And (intCustomDataTypeCounter = 0) And (intExcludedDataTypesCounter = 0) Then
				bolOnlineAvailable = True
			End If

			' Additionally, a package variable allowing for explicit override of the ONLINE option exists to provide direct control over how the index is defragmented.  Regardless of the three options, if the defragmentation job parameter is TRUE (indicating override of all ONLINE operations), then set the option to FALSE.  This applies to ALL indexes and not just indexes required to be offline and to all SQL editions 
			If CBool(Dts.Variables("User::Defragment_Parameters_OverrideOnlineOps").Value.ToString) = True Then
                bolOnlineAvailable = False
            ElseIf intIndexType > 2 Then
                bolOnlineAvailable = False

            End If

            'Because values need to determine IF an index is subject to defragmentation, this step can be performed prior to any other work or script logic
            If (Round(dblCurrentFragmentationPercent, 0) > Round(dblMinimumFragmenationParameter, 0)) Then
                If (intCurrentPageCount >= intMinPageCountParameter) Then
                    ' A negative number in max days may result in undesired effects.  Minimum value is 0
                    If intMinDefragDateDiffParameter < 0 Then
                        intMinDefragDateDiffParameter = 0
                    End If
                    If intDaysDifferent > intMinDefragDateDiffParameter Then
                        ' Special Note: the equation uses just GREATER THAN rather than GREATER AND EQUAL TO because, 
                        ' the max number of days COULD BE 0, this effectively ignores ANY age and all indexes (meeting the prior constraints) are processed.
                        ' HOWEVER, to enable restarting of a defragment job (within the same day due to failures), if the equation used >=, then indexes processed previously within the same day could be subject to defragmentation again.  While unlikely percentage fragmented would change, it is possible.  Therefore to prevent "churn", the date difference must be at least 1 day even if the threshold is 0).
                        ' Since all three conditions are met for whether an Index should be defragmented, determination of the second set of factors (HOW the index will be defragmented) is performed.  There are four factors: 					

                        If objTable.Indexes IsNot Nothing And objTable.Indexes.Count > 0 Then
                            ' Set the SMO Index object 
                            objIndex = objTable.Indexes(strIndexName)
                            '  Next perform the first two validations
                            dtIndexDefragStartTime = Now()
                            strIndexDetailComments = "All criteria met. Defragmentation started--" + strConnInformation + ": Index Type:" + intIndexType.ToString() + vbCrLf
                            
                            If (dblCurrentFragmentationPercent >= dblRebuildIndexThreshold) Then
                                objIndex.SortInTempdb = True
                                objIndex.OnlineIndexOperation = bolOnlineAvailable
                                objIndex.Rebuild()
                                bolIndexReorgPerformed = False
                                bolIndexRebuildPerformed = True
                            Else
                                objIndex.SortInTempdb = True
                                objIndex.OnlineIndexOperation = bolOnlineAvailable
                                objIndex.Reorganize()
                                bolIndexReorgPerformed = True
                                bolIndexRebuildPerformed = False

                            End If
                            ' The cummulative/running count of processed indexes is incremented by 1 if the defragmentation completes successfully
                            bolDefragmentationFailedInd = False
                            bolIndexDefragPerformed = True
                            intRunningProcessedIndexesCount = intRunningProcessedIndexesCount + 1
                            strIndexDetailComments = "DEFRAGMENTATION COMPLETE: " + strIndexDetailComments

                        Else '-- else of objTable index validation
                            ' Table has no indexes.  This step should never fire (or it should throw an error) as the FELC Indexes only contains tables which have indexes.  This validation should always pass
                            '  Variables values this index do not need to be reset as the values set in the beginning of the script are the current values
                            '  However, not performing defragmentation for this reason is NOT a fatal error
                            strIndexDetailComments = "NO DEFRAGMENTATION DONE: Table has no indexes"
                            bolDefragmentationFailedInd = False
                            Throw New Exception(strIndexDetailComments)
                        End If '-- end if of objTable index validation


                    Else '--else of time since last deframentation performed
                        '  the number of days since the last defragmentation for this index is not greater than the cutoff days--do not process and do not add to the processed or failed counts but log entry in details table as to why this was not processed.
                        '  Defragmenation performed--False
                        '  Rebuild performed - FALSE
                        '  Reorg performed = FALSE
                        '  Post Run defrag percent = prerun defrag percent (which is the current item variable value)
                        '  Post run page count = prerun defrag percent (which is the current item variable value)
                        '  Defrag end time = defrag start time
                        '  Variables values this index do not need to be reset as the values set in the beginning of the script are the current values
                        '  However, not performing defragmentation for this reason is NOT a fatal error
                        strIndexDetailComments = "NO DEFRAGMENTATION DONE: Index did not meet date cutoff: Index Type:" + intIndexType.ToString() + ""
                        bolDefragmentationFailedInd = False
                        Throw New Exception(strIndexDetailComments)
                    End If '--else of time since last deframentation performed

                Else '--else of page count validation
                    '  page count of current index does not meet threshold--do not process and do not add to the processed or failed counts but log entry in details table as to why this was not processed.
                    '  Defragmenation performed--False
                    '  Rebuild performed - FALSE
                    '  Reorg performed = FALSE
                    '  Post Run defrag percent = prerun defrag percent (which is the current item variable value)
                    '  Post run page count = prerun defrag percent (which is the current item variable value)
                    '  Defrag end time = defrag start time
                    '  Variables values this index do not need to be reset as the values set in the beginning of the script are the current values
                    '  However, not performing defragmentation for this reason is NOT a fatal error
                    strIndexDetailComments = "NO DEFRAGMENTATION DONE: Index did not meet minimum page count: Index Type:" + intIndexType.ToString()
                    bolDefragmentationFailedInd = False
                    Throw New Exception(strIndexDetailComments)
                End If '--end if of page count validation
            Else  ' -- else of fragmentation threshold check
                '  the index fragmentation did not exceed the minimum threshold--do not process and do not add to the processed or failed counts but log entry in details table as to why this was not processed.
                '  Defragmenation performed--False
                '  Rebuild performed - FALSE
                '  Reorg performed = FALSE
                '  Post Run defrag percent = prerun defrag percent (which is the current item variable value)
                '  Post run page count = prerun defrag percent (which is the current item variable value)
                '  Defrag end time = defrag start time
                '  Variables values this index do not need to be reset as the values set in the beginning of the script are the current values
                '  However, not performing defragmentation for this reason is NOT a fatal error
                strIndexDetailComments = "NO DEFRAGMENTATION DONE: Index did not meet minimum fragmentation percentage cut off: Index Type:" + intIndexType.ToString()
                bolDefragmentationFailedInd = False
                Throw New Exception(strIndexDetailComments)
            End If ' -- end if of fragmentation threshold check

        Catch ex As Exception
            Dim strInnerMsg As String = "Inner Exception:"
            Do While ex.InnerException IsNot Nothing
                strInnerMsg = strInnerMsg & ex.InnerException.Message.ToString
                ex = ex.InnerException
            Loop
            strExceptionMessage = ex.Message + vbTab + strInnerMsg
            If bolDefragmentationFailedInd = True Then
                intRunningFailedIndexesCount = intRunningFailedIndexesCount + 1
                strIndexDetailComments = "OPERATION FAILED: " + strExceptionMessage
                Dts.Events.FireError(0, strTaskName, strExceptionMessage, String.Empty, 0)
                bolDTSTaskResults = DTSExecResult.Failure
            Else
                strIndexDetailComments = strExceptionMessage
                'Dts.Events.FireWarning(0, strTaskName, strExceptionMessage, String.Empty, 0)
                bolDTSTaskResults = DTSExecResult.Success
            End If

        Finally
            dtIndexDefragEndTime = Now()
            Dts.Variables("User::ErrorHandling_FatalErrors_Count").Value = intRunningFECount
            Dts.Variables("User::ErrorHandling_NonFatalErrors_Count").Value = intRunningNFECount
            Dts.Variables("User::Table_RowCount").Value = CInt(intTableRowCount)
            Dts.Variables("User::IndexHistory_Rebuild_PerformedbyPriorJob_Ind").Value = bolIndexRebuildPerformed
            Dts.Variables("User::IndexHistory_ReorgPerformedbyPriorJob_Ind").Value = bolIndexReorgPerformed
            Dts.Variables("User::IndexHistory_PriorDefragmentPerformed_Ind").Value = bolIndexDefragPerformed
            Dts.Variables("User::IndexHistory_IndexDefragStartDate").Value = dtIndexDefragStartTime
            Dts.Variables("User::IndexHistory_IndexDefragEndDate").Value = dtIndexDefragEndTime
            Dts.Variables("User::Indexes_Processed_Count").Value = intRunningProcessedIndexesCount
            Dts.Variables("User::ErrorHandling_FailedIndexes_TotalCount").Value = intRunningFailedIndexesCount
            Dts.Variables("User::Defragment_Detail_Comments").Value = strIndexDetailComments
            Dts.Variables("User::IndexHistory_DefragmentFailedInd").Value = bolDefragmentationFailedInd

            connSMO.Disconnect()
            objServer = Nothing
            connSMO = Nothing
        End Try

		Dts.TaskResult = bolDTSTaskResults

	End Sub


End Class]]></ProjectItem>
