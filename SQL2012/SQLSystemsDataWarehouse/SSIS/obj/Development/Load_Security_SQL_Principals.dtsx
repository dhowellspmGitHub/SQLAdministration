            SQLTask:ResultName="0"
            SQLTask:DtsVariableName="User::ETL_ProcessControl_Id" />
          <SQLTask:ResultBinding
            SQLTask:ResultName="3"
            SQLTask:DtsVariableName="User::ETL_ProcessControl_Date" />
          <SQLTask:ResultBinding
            SQLTask:ResultName="6"
            SQLTask:DtsVariableName="User::ETLProcess_RestartInd" />
          <SQLTask:ResultBinding
            SQLTask:ResultName="7"
            SQLTask:DtsVariableName="User::ETL_Process_PriorId" />
          <SQLTask:ParameterBinding
            SQLTask:ParameterName="@P1"
