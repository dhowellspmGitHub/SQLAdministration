      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{420DA5B7-669E-4B96-B18A-18AE83CE077B}"
          SQLTask:SqlStatementSource="DECLARE @CurrTS AS DateTime&#xA;SET @CurrTS = GETDATE()&#xA;&#xA;--Get database count for &#xA;SELECT @totaldbcount = COUNT([database_name])&#xA;FROM [dbo].[DBCC_History_Detail]&#xA;WHERE [DBCC_Job_ID] = @P1&#xA;&#xA;SELECT @processdbcount = COUNT([database_name])&#xA;FROM [dbo].[DBCC_History_Detail]&#xA;WHERE [DBCC_Job_ID] = @P1&#xA;AND [DBCC_performed_ind] = 1&#xA;&#xA;SELECT  @completedcount = COUNT([database_name])&#xA;FROM [dbo].[DBCC_History_Detail]&#xA;WHERE [DBCC_Job_ID] = @P1&#xA;AND [DBCC_performed_ind] = 1&#xA;AND [DBCC_completed_ind] = 1&#xA;&#xA;SELECT @faileddbcount = COUNT([database_name])&#xA;FROM [dbo].[DBCC_History_Detail]&#xA;WHERE [DBCC_Job_ID] = @P1&#xA;AND [DBCC_performed_ind] = 1&#xA;AND [DBCC_completed_ind] = 0&#xA;&#xA;SELECT @totaldbcount = ISNULL(@totaldbcount,0)&#xA;,@processdbcount = ISNULL(@processdbcount,0)&#xA;,@completedcount = ISNULL(@completedcount,0)&#xA;,@faileddbcount = ISNULL(@faileddbcount,0)&#xA;&#xA;SELECT @totaldbcount, @processdbcount, @completedcount, @faileddbcount&#xA;&#xA;&#xA;UPDATE [dbo].[DBCC_History_Summary]&#xA;   SET [DBCC_Job_Endtime] = @CurrTS &#xA;      ,[Databases_Total_Count] =  @totaldbcount &#xA;      ,[Databases_Completed_Count] = @completedcount&#xA;      ,[Databases_Failed_Count] = @faileddbcount&#xA;      ,[DBCC_Summary_Comments] = ('DBCC Processing Finished;' + [DBCC_Summary_Comments])&#xA; WHERE [DBCC_Job_ID] = @P1&#xA;&#xA;&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
          <SQLTask:ParameterBinding
            SQLTask:ParameterName="@P1"
            SQLTask:DtsVariableName="User::DBCC_Summary_Job_ID"
            SQLTask:ParameterDirection="Input"
            SQLTask:DataType="11"
            SQLTask:ParameterSize="-1" />
          <SQLTask:ParameterBinding
            SQLTask:ParameterName="@totaldbcount"
            SQLTask:DtsVariableName="User::Databases_TotalCount"
            SQLTask:ParameterDirection="Output"
            SQLTask:DataType="11"
            SQLTask:ParameterSize="-1" />
          <SQLTask:ParameterBinding
            SQLTask:ParameterName="@completedcount"
            SQLTask:DtsVariableName="User::Databases_ProcessCompletedCount"
            SQLTask:ParameterDirection="Output"
            SQLTask:DataType="11"
            SQLTask:ParameterSize="-1" />
          <SQLTask:ParameterBinding
            SQLTask:ParameterName="@processdbcount"
            SQLTask:DtsVariableName="User::Databases_ProcessedCount"
            SQLTask:ParameterDirection="Output"
            SQLTask:DataType="11"
            SQLTask:ParameterSize="-1" />
          <SQLTask:ParameterBinding
            SQLTask:ParameterName="@faileddbcount"
            SQLTask:DtsVariableName="User::ErrorHandling_FailedDatabases_Count"
