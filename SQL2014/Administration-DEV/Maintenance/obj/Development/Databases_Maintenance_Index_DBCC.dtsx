                    Encoding="UTF8"><![CDATA[' Microsoft SQL Server Integration Services Script Task
' Write scripts using Microsoft Visual Basic 2008.
' The ScriptMain is the entry point class of the script.

Imports System
Imports System.Data
Imports System.Math
Imports Microsoft.SqlServer.Dts.Runtime

<Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute> _
<System.CLSCompliantAttribute(False)> _
Partial Public Class ScriptMain
	Inherits Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase

	Enum ScriptResults
		Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
		Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
	End Enum
	
   
    Public Sub Main()
		'RO Variables: System::ErrorDescription,System::SourceName,$Project::Database_Name_01,$Project::Server_Name_01
		'RW Variables: User::ErrorHandling_NotificationText

		Dim strPersistentText As String = String.Empty
		strPersistentText = "The package failed at the task:" & vbTab & Dts.Variables("System::SourceName").Value.ToString & vbCrLf & _
		 "Project parameters values:" & vbCrLf & _
		 "Server_Name_01:" & vbTab & Dts.Variables("$Project::Server_Name_01").Value.ToString & vbCrLf & _
		 "Database_Name_01:" & vbTab & Dts.Variables("$Project::Database_Name_01").Value.ToString & vbCrLf & _
	   "Error Information: The server or database are not available or have invalid values." & vbCrLf & _
	   "Package Error Description:" & vbTab & Dts.Variables("System::ErrorDescription").Value.ToString

		Dts.Variables("User::ErrorHandling_NotificationText").Value = Left(strPersistentText, 4000)


		Dts.TaskResult = ScriptResults.Success
    End Sub

End Class]]></ProjectItem>
