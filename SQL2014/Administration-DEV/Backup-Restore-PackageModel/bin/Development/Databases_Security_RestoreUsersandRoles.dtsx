	AND [DatabaseName] = @DatabaseName
	AND [Backup_Principals_Job_ID] = @Backup_Job_ID

	SET @Restore_Principals_Performed_Ind = 1
	SET @Restore_Principals_Failed_Ind = 1
	SET @Restore_Principals_Comments = 'FAILED: Database not in list of excluded databases.  Database not in a state capable of restoring credentials, roles and authorizations'
END
