          <ProjectItem
            Name="ScriptMain.vb"
            Encoding="UTF8"><![CDATA[' Microsoft SQL Server Integration Services Script Task
' Write scripts using Microsoft Visual Basic 2008.
' The ScriptMain is the entry point class of the script.

Imports System
Imports System.Data
Imports System.Math
Imports Microsoft.SqlServer.Dts.Runtime

<Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute> _
<System.CLSCompliantAttribute(False)> _
Partial Public Class ScriptMain
	Inherits Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase

	Enum ScriptResults
		Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
		Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
	End Enum
    Public Sub Main()
        'System::PackageName
        'User::Database_Name_01,User::Database_Name_02,User::ErrorHandling_Email_CC,User::ErrorHandling_Email_Message,User::ErrorHandling_Email_Subject,User::ErrorHandling_Email_ToLine,User::ErrorHandling_ErrorCount,User::ErrorHandling_PackageFailed,User::ErrorHandling_SendEmailOnSuccess,User::ErrorHandling_TaskResults,User::Server_Name_01,User::Server_Name_02

        ' This script is to complete the build of text fields and set notification variables used Email--Send completion status task
        Dim strEmailTaskOutcomes As String = Dts.Variables("User::ErrorHandling_TaskResults").Value.ToString
        Dim strEmailSubject As String
        Dim strEmailMessage As String = String.Empty
        Dim strEmailCC As String
        Dim strEmailMessageFooter As String = "Please review the package for logging options used.  Please see job and SQL Integration Services log reports, if enabled, for more information."

        Dim strPackagName As String = Dts.Variables("System::PackageName").Value.ToString
        Dim bolPackageFailed As Boolean = CBool(Dts.Variables("User::ErrorHandling_PackageFailed").Value.ToString)
        Dim strPackageCompletionState As String
        Dim A As String = String.Empty

        
        If bolPackageFailed = False Then
            ' the package did not fail--set email text values accordingly
            strPackageCompletionState = "SUCCESS"
            strEmailSubject = strPackageCompletionState & "--" + Dts.Variables("System::PackageName").Value.ToString + " SSIS Package"
            strEmailMessage = "PACKAGE " & strPackageCompletionState & "--All tasks in the " & Dts.Variables("System::PackageName").Value.ToString & " package completed successfully.  Detailed information is:" & vbCrLf & vbCrLf & strEmailTaskOutcomes & vbCrLf & vbCrLf & strEmailMessageFooter & vbCrLf & _
            "Logging mode:" & A
            strEmailCC = ""

        Else
            ' the package did fail--set email text values accordingly
            strPackageCompletionState = "FAILURE"
            strEmailSubject = strPackageCompletionState & "--" + Dts.Variables("System::PackageName").Value.ToString + " SSIS Package"
            strEmailMessage = "PACKAGE " & strPackageCompletionState & "--At least one task in the " & Dts.Variables("System::PackageName").Value.ToString & " package failed successfully.  Detailed information is:" & vbCrLf & strEmailTaskOutcomes & vbCrLf & vbCrLf & strEmailMessageFooter & vbCrLf & _
            "Logging mode:" & A

            strEmailCC = "sql_alerts@kyfb.com"
            ' No need to send Email CC if the notification is going to same address
            If InStr(strEmailCC, Dts.Variables("User::ErrorHandling_Email_ToLine").Value.ToString, CompareMethod.Text) <> 0 Then
                strEmailCC = ""
            End If
            ' The next task will send an email if the variable User::ErrorHandling_SendEmailOnSuccess is set to TRUE.
            ' Since failure emails should always be sent, override the disabling of email sending
            Dts.Variables("User::ErrorHandling_SendEmailOnSuccess").Value = True

        End If
        Dts.Variables("User::ErrorHandling_Email_Subject").Value = strEmailSubject
        Dts.Variables("User::ErrorHandling_Email_Message").Value = strEmailMessage
        Dts.Variables("User::ErrorHandling_Email_CC").Value = strEmailCC
        Dts.TaskResult = ScriptResults.Success
    End Sub

End Class]]></ProjectItem>
