                            Encoding="UTF8"><![CDATA[' Microsoft SQL Server Integration Services Script Task
' Write scripts using Microsoft Visual Basic 2008.
' The ScriptMain is the entry point class of the script.

Imports System
Imports System.Data
Imports System.Math
Imports Microsoft.SqlServer.Dts.Runtime

<Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute> _
<System.CLSCompliantAttribute(False)> _
Partial Public Class ScriptMain
    Inherits Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase

    Enum ScriptResults
        Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
        Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
    End Enum

    Public Sub Main()
        ' User::DatabasePermission_ClassDesc,User::DatabasePermission_ObjectName,User::DatabasePermission_PermissionName,User::DatabasePermission_SchemaName,User::DatabasePermission_StateDesc,User::DatabaseRole_Name,User::ErrorHandling_TaskResults,User::TSQL_Stmt

        Dim strClassDesc As String = Dts.Variables("User::DatabasePermission_ClassDesc").Value.ToString
        Dim strSchemaName As String = Dts.Variables("User::DatabasePermission_SchemaName").Value.ToString
        Dim strObjectName As String = Dts.Variables("User::DatabasePermission_ObjectName").Value.ToString
        Dim strPermissionName As String = Dts.Variables("User::DatabasePermission_PermissionName").Value.ToString
        Dim strStateDesc As String = Dts.Variables("User::DatabasePermission_StateDesc").Value.ToString
        Dim strGrantee As String = Dts.Variables("User::DatabaseRole_Name").Value.ToString
        Dim strTSQL As String = String.Empty
        'Error Handling
        Dim strSourceName As String = Dts.Variables("System::TaskName").Value.ToString
        Dim strTaskResults As String = Dts.Variables("User::ErrorHandling_TaskResults").Value.ToString
        Dim strEventLogDesc As String = String.Empty
        Dim objTaskResults As DTSExecResult = DTSExecResult.Success

        Try
            ' It is assumed that the database exists and is online and that its state did not change since the creation of the role in the previous task
            ' Therefore no confirmation of either is done prior to the creation and execution of the statements recreating access.
            If StrComp("DATABASE", UCase(strClassDesc)) = 0 Then
                strTSQL = strStateDesc + " " + strPermissionName + " TO " + "[" + strGrantee + "]"
            Else
                strTSQL = "IF  EXISTS (SELECT * FROM sys.objects WHERE object_id = OBJECT_ID(N'[" + strSchemaName + "].[" + strObjectName + "]'))" + vbCrLf
                strTSQL = strTSQL + strStateDesc + " " + strPermissionName + " ON [" + strSchemaName + "].[" + strObjectName + "] TO [" + strGrantee + "]"
            End If
            Dts.Variables("User::TSQL_Stmt").Value = strTSQL
        Catch ex As Exception
            'Exception handling 
            Dim strInnerMsg As String = "Inner Exception:"
            Do While ex.InnerException IsNot Nothing
                strInnerMsg &= ex.InnerException.Message.ToString
                ex = ex.InnerException
            Loop
            strEventLogDesc = ex.Message & vbTab & strInnerMsg & "ROLE: " & strGrantee & "--error creating the the TSQL value and variable used to assign custom role permissions"
            strTaskResults = strTaskResults & vbCrLf & "ERROR: " & strSourceName & "--" & strEventLogDesc

            Dts.Variables("User::TSQL_Stmt").Value = ""
            Dts.Variables("User::ErrorHandling_TaskResults").Value = strTaskResults
            objTaskResults = DTSExecResult.Failure
        End Try

        Dts.TaskResult = objTaskResults
    End Sub

End Class]]></ProjectItem>
