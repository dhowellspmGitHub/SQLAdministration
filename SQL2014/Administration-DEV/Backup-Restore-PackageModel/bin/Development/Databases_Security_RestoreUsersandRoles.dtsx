	SET @Restore_Principals_Failed_Count = 0
	SET @Restore_CustomRoles_Failed_Count = 0
	SET @Restore_DatabaseRolemembers_Failed_Count = 0
	SET @Restore_CustomRolePermissions_Failed_Count = 0
	SET @Restore_Principals_Performed_Ind = 0
