						objDatabaseExtProperty = New Microsoft.SqlServer.Management.Smo.ExtendedProperty
						If colDatabaseExtProperties.Contains(strExtPropName) = False Then
							objDatabaseExtProperty.Parent = objServer.Databases(objDatabase.Name.ToString)
							objDatabaseExtProperty.Name = strExtPropName
							objDatabaseExtProperty.Value = strExtPropValue
							objDatabaseExtProperty.Create()
						Else
							' The database already has this extended property.  Need to update the value of it to the value captured before the restore
							'objDatabaseExtProperty.Parent = objServer.Databases(objDatabase.Name.ToString)
							'objDatabaseExtProperty.Name = strExtPropName
							'objDatabaseExtProperty.Value = strExtPropValue
							objDatabase.Alter()
							objDatabase.ExtendedProperties.Item(strExtPropName).Value = strExtPropValue
							objDatabase.Alter()
							'objDatabaseExtProperty.Alter()

						End If
					Else
						' The database has no extended properties--this means, for the KFB SQL Mgmt usage, the Extended Properties used for that purpose need to be created
						' This condition could occur when a database is restored from a backup generated before this functionality was implemented.
						' Add the designated Database Extended property (ApplicationID)
						objDatabaseExtProperty = New Microsoft.SqlServer.Management.Smo.ExtendedProperty
						objDatabaseExtProperty.Parent = objServer.Databases(objDatabase.Name.ToString)
						objDatabaseExtProperty.Name = strExtPropName
						objDatabaseExtProperty.Value = strExtPropValue
						objDatabaseExtProperty.Create()
					End If
                Else
                    ' Database is not accessible--fail package
                    bolNonFatalErrorInd = False
                    strEventLogDesc = "ERROR: Task: " & strEventSubComp & ":Database " & strDatabaseName & " is not accessible at this time."
                    Throw New Exception(strEventLogDesc)
                End If
            Else
                ' Database does not exist--fail package
                bolNonFatalErrorInd = False
                strEventLogDesc = "ERROR: Task: " & strEventSubComp & ": Database " & strDatabaseName & " does not exists on server " & objServer.Name.ToString
                Throw New Exception(strEventLogDesc)
            End If

        Catch ex As Exception
            Dts.Variables("User::ErrorHandling_PackageFailed").Value = True
            Dim strInnerMsg As String = String.Empty
            Do While ex.InnerException IsNot Nothing
                strInnerMsg = strInnerMsg & ex.InnerException.Message.ToString
                ex = ex.InnerException
            Loop
            strEventLogDesc = ex.Message & vbTab & strInnerMsg
            If bolNonFatalErrorInd = True Then
                Dts.Events.FireWarning(0, strEventSubComp, strEventLogDesc, String.Empty, 0)
                objTaskResults = DTSExecResult.Completion
            Else
                Dts.Events.FireError(0, strEventSubComp, strEventLogDesc, String.Empty, 0)
                objTaskResults = DTSExecResult.Failure
            End If
            strTaskResults = strTaskResults & vbCrLf & strEventLogDesc
        Finally
            Dts.Variables("User::ErrorHandling_TaskResults").Value = strTaskResults
        End Try
        objDatabase = Nothing
        objServer = Nothing

        Dts.TaskResult = objTaskResults
    End Sub

End Class]]></ProjectItem>
