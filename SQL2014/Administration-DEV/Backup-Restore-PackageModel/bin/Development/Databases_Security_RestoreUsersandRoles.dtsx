    Public Sub Main()
        'User::Database_Name_02,User::DatabaseUser_DefaultSchema,User::DatabaseUser_Name,User::DatabaseUser_ServerPrincipal,User::DatabaseUser_Type
        'SMO variables
        Dim objServer As Microsoft.SqlServer.Management.Smo.Server
        Dim objDatabase As Database
        Dim strDatabaseName As String = Dts.Variables("User::Database_Name_02").Value.ToString
        Dim objUser As User

        Dim strDefaultSchema As String = Dts.Variables("User::DatabaseUser_DefaultSchema").Value.ToString
        Dim strUserName As String = Dts.Variables("User::DatabaseUser_Name").Value.ToString
        Dim strUserType As String = Dts.Variables("User::DatabaseUser_Type").Value.ToString
        Dim strServerPrincipal As String = Dts.Variables("User::DatabaseUser_ServerPrincipal").Value.ToString

        'Error Handling variables
        Dim strEventLogDesc As String = String.Empty
        Dim strEventSubComp As String = Dts.Variables("System::TaskName").Value.ToString
        Dim objTaskResults As DTSExecResult = DTSExecResult.Success
        Dim bolNonFatalErrorInd As Boolean = False
        Dim strTaskResults As String = Dts.Variables("User::ErrorHandling_TaskResults").Value.ToString

        Try
            objServer = DirectCast(Dts.Connections("SMO.Server_02").AcquireConnection(Dts.Transaction), Microsoft.SqlServer.Management.Smo.Server)
            If objServer.Databases.Contains(strDatabaseName) Then
                objDatabase = objServer.Databases(strDatabaseName)
                If objDatabase.IsAccessible = True Then
                    If objServer.Logins.Contains(strServerPrincipal) = True Then
                        If objDatabase.Users.Contains(strUserName) = False Then
                            objUser = New User(objDatabase, strServerPrincipal)
                            objUser.Login = strUserName
                            ' Create default schema if it does not exist
                            If objDatabase.Schemas.Contains(strDefaultSchema) = False And StrComp(strDefaultSchema, "dbo") <> 0 Then
                                Dim z As Schema = New Schema(objDatabase, strDefaultSchema)
                                z.Create()
                            End If
                            ' Windows Groups do not have default schemas--only set if type is not Windows group
                            If StrComp("G", strUserType) <> 0 Then
                                objUser.DefaultSchema = strDefaultSchema
                            End If
                            'Create the database principal
                            objUser.Create()
                            strEventLogDesc = "COMPLETED: Task: " & strEventSubComp & "--" & strUserName & "--user created"
                            Dts.Events.FireInformation(0, strEventSubComp, strEventLogDesc, String.Empty, 0, False)
                            ' Else the user exists--which should not occur as all database users were dropped in the prior task
                        End If
                    Else
                        ' Database is not accessible--fail package
                        bolNonFatalErrorInd = True
                        strEventLogDesc = "ERROR: Task: " & strEventSubComp & ": " & strUserName & " server login does not exist for the user."
                        Throw New Exception(strEventLogDesc)
                    End If
                Else
                    ' Database is not accessible--fail package
                    bolNonFatalErrorInd = False
                    strEventLogDesc = "ERROR: Task: " & strEventSubComp & ":Database " & strDatabaseName & " is not accessible at this time."
                    Throw New Exception(strEventLogDesc)
                End If
            Else
                ' Database does not exist--fail package
                bolNonFatalErrorInd = False
                strEventLogDesc = "ERROR: Task: " & strEventSubComp & ": Database " & strDatabaseName & " does not exists on server " & objServer.Name.ToString
                ' The failure to create a single database role should not fail the entire package.  A custom OnError event handler is required to continue the package
                Throw New Exception(strEventLogDesc)
            End If

        Catch ex As Exception
            Dts.Variables("User::ErrorHandling_PackageFailed").Value = True
            Dim strInnerMsg As String = String.Empty
            Do While ex.InnerException IsNot Nothing
                strInnerMsg = strInnerMsg & ex.InnerException.Message.ToString
                ex = ex.InnerException
            Loop
            strEventLogDesc = ex.Message & vbTab & strInnerMsg
            If bolNonFatalErrorInd = True Then
                Dts.Events.FireWarning(0, strEventSubComp, strEventLogDesc, String.Empty, 0)
                objTaskResults = DTSExecResult.Completion
            Else
                Dts.Events.FireError(0, strEventSubComp, strEventLogDesc, String.Empty, 0)
                objTaskResults = DTSExecResult.Failure
            End If

        Finally
            strTaskResults = strTaskResults & vbCrLf & strEventLogDesc
            Dts.Variables("User::ErrorHandling_TaskResults").Value = strTaskResults
        End Try
        objUser = Nothing
        objDatabase = Nothing
        objServer = Nothing
        Dts.TaskResult = objTaskResults
    End Sub

End Class]]></ProjectItem>
